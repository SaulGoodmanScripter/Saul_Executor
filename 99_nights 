        end
    })

    -- Auto Chop Tree
    GameTab:Toggle({
        Title = "üå≥ Auto Chop Tree",
        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —Ä—É–±–∫–∞ –¥–µ—Ä–µ–≤—å–µ–≤",
        Value = false,
        Callback = function(v)
            ActiveAutoChopTree = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Auto Chop",
        Desc = "–†–∞–¥–∏—É—Å —Ä—É–±–∫–∏ –¥–µ—Ä–µ–≤—å–µ–≤",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
            DistanceForAutoChopTree = val
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê BRING ITEMS
local BringItemTab = Window:Tab({Title = "üì¶ Bring Items", Icon = "package"}) do
    BringItemTab:Section({Title = "–°–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    local function BringItemsByName(name)
        if DoingTheThings then return end
        DoingTheThings = true
        OldCFrameHere = LocalPlayer.Character.HumanoidRootPart.CFrame
        
        for _, Obj in pairs(Workspace.Items:GetChildren()) do
            if Obj.Name == name and Obj:IsA("Model") and Obj.PrimaryPart then 
                LocalPlayer.Character.HumanoidRootPart.CFrame = Obj.PrimaryPart.CFrame
                DragItem(Obj)
                wait(0.15)
            end 
        end
        
        DoingTheThings = false
        LocalPlayer.Character.HumanoidRootPart.CFrame = OldCFrameHere
    end

    local function BringItemsByType(itemTypes)
        if DoingTheThings then return end
        DoingTheThings = true
        OldCFrameHere = LocalPlayer.Character.HumanoidRootPart.CFrame
        
        for _, Obj in pairs(Workspace.Items:GetChildren()) do
            if Obj:IsA("Model") and Obj.PrimaryPart then
                local found = false
                for _, itemType in ipairs(itemTypes) do
                    if Obj.Name == itemType then
                        found = true
                        break
                    end
                end
                if found then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Obj.PrimaryPart.CFrame
                    DragItem(Obj)
                    wait(0.15)
                end
            end   
        end
        
        DoingTheThings = false
        LocalPlayer.Character.HumanoidRootPart.CFrame = OldCFrameHere
    end

    BringItemTab:Button({
        Title = "üì¶ –í—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã",
        Desc = "–ü–µ—Ä–µ–Ω–µ—Å—Ç–∏ –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã",
        Callback = function()
            if DoingTheThings then return end
            DoingTheThings = true
            OldCFrameHere = LocalPlayer.Character.HumanoidRootPart.CFrame
            
            for _, Obj in pairs(Workspace.Items:GetChildren()) do
                if Obj:IsA("Model") and Obj.PrimaryPart then 
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Obj.PrimaryPart.CFrame
                    DragItem(Obj)
                    wait(0.15)
                end  
            end
            
            DoingTheThings = false
            LocalPlayer.Character.HumanoidRootPart.CFrame = OldCFrameHere
        end
    })

    BringItemTab:Section({Title = "–ö–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ –ø—Ä–µ–¥–º–µ—Ç—ã"})

    local itemsList = {
        {"ü™µ –í—Å–µ –±—Ä–µ–≤–Ω–∞", "Log"},
        {"‚ö´Ô∏è –í–µ—Å—å —É–≥–æ–ª—å", "Coal"},
        {"‚õΩÔ∏è –í—Å–µ –∫–∞–Ω–∏—Å—Ç—Ä—ã", "Fuel Canister"},
        {"ü•ï –í—Å–µ –º–æ—Ä–∫–æ–≤–∫–∏", "Carrot"},
        {"ü©π –í—Å–µ –±–∏–Ω—Ç—ã", "Bandage"},
        {"üíä –í—Å–µ –∞–ø—Ç–µ—á–∫–∏", "MedKit"},
        {"üìª –í—Å–µ —Å—Ç–∞—Ä—ã–µ —Ä–∞–¥–∏–æ", "Old Radio"},
        {"üõû –í—Å–µ —à–∏–Ω—ã", "Tyre"},
        {"üåÄ –í—Å–µ —Å–ª–æ–º–∞–Ω–Ω—ã–µ –≤–µ–Ω—Ç–∏–ª—è—Ç–æ—Ä—ã", "Broken Fan"},
        {"üß≤ –í—Å–µ –±–æ–ª—Ç—ã", "Bolt"},
        {"üî© –í—Å–µ –º–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–µ –ª–∏—Å—Ç—ã", "Sheet Metal"},
        {"üå± –í—Å–µ –∫–æ—Ä–æ–±–∫–∏ —Å–µ–º—è–Ω", "Seed Box"},
        {"ü™ë –í—Å–µ —Å—Ç—É–ª—å—è", "Chair"},
        {"üç∞ –í—Å–µ —Ç–æ—Ä—Ç—ã", "Cake"},
        {"ü•™ –í—Å–µ —Å—ç–Ω–¥–≤–∏—á–∏", "Meat? Sandwich"}
    }

    for _, itemData in ipairs(itemsList) do
        BringItemTab:Button({
            Title = itemData[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å " .. itemData[2],
            Callback = function()
                BringItemsByName(itemData[2])
            end
        })
    end

    BringItemTab:Section({Title = "–ì—Ä—É–ø–ø—ã –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    local groupItems = {
        {"üõ¢Ô∏è –í—Å–µ —Ç–æ–ø–ª–∏–≤–æ", {"Log", "Fuel Canister", "Coal", "Oil Barrel"}},
        {"üî© –í—Å–µ –∑–∞–ø—á–∞—Å—Ç–∏", {"Tyre", "Sheet Metal", "Broken Fan", "Bolt", "Old Radio", "UFO Junk", "UFO Scrap", "Broken Microwave"}},
        {"üî´ –í—Å–µ –ø–∞—Ç—Ä–æ–Ω—ã", {"Rifle Ammo", "Revolver Ammo"}},
        {"üçé –í—Å—è –µ–¥–∞", {"Cake", "Carrot", "Morsel", "Meat? Sandwich"}}
    }

    for _, groupData in ipairs(groupItems) do
        BringItemTab:Button({
            Title = groupData[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å –≥—Ä—É–ø–ø—É –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
            Callback = function()
                BringItemsByType(groupData[2])
            end
        })
    end

    BringItemTab:Button({
        Title = "üë∂ –í—Å–µ –¥–µ—Ç–∏",
        Desc = "–ü–µ—Ä–µ–Ω–µ—Å—Ç–∏ –≤—Å–µ—Ö –¥–µ—Ç–µ–π",
        Callback = function()
            if DoingTheThings then return end
            DoingTheThings = true
            OldCFrameHere = LocalPlayer.Character.HumanoidRootPart.CFrame
            
            for _, Obj in pairs(Workspace.Characters:GetChildren()) do
                if Obj:IsA("Model") and Obj.PrimaryPart and (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4") then 
                    LocalPlayer.Character.HumanoidRootPart.CFrame = Obj.PrimaryPart.CFrame
                    DragItem(Obj)
                    wait(0.15)
                end  
            end
            
            DoingTheThings = false
            LocalPlayer.Character.HumanoidRootPart.CFrame = OldCFrameHere
        end
    })

    BringItemTab:Section({Title = "–ü–æ–∏—Å–∫ –ø–æ –∏–º–µ–Ω–∏"})

    local itemSearchLabel = BringItemTab:Label({
        Title = "–ü–æ–∏—Å–∫ –ø—Ä–µ–¥–º–µ—Ç–∞",
        Desc = "–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞"
    })

    local itemSearchBox = BringItemTab:Textbox({
        Title = "üîç –ü–æ–∏—Å–∫ –ø—Ä–µ–¥–º–µ—Ç–∞",
        Desc = "–ù–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞ (–∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ ESP)",
        Placeholder = "–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ...",
        Value = "",
        ClearText = false,
        Callback = function(text)
            TextBoxText = text
            isInTheMap = "–Ω–µ—Ç"
            HowManyItemCanShowUp = 0
            
            for _, Obj in pairs(Workspace.Items:GetChildren()) do
                if Obj.Name == TextBoxText and Obj:IsA("Model") and Obj.PrimaryPart then 
                    HowManyItemCanShowUp = HowManyItemCanShowUp + 1
                    isInTheMap = "–¥–∞"
                end 
            end
            
            itemSearchLabel:SetDesc(string.format("–ü—Ä–µ–¥–º–µ—Ç –Ω–∞–π–¥–µ–Ω: %s (x%d)", isInTheMap, HowManyItemCanShowUp))
        end
    })

    BringItemTab:Button({
        Title = "üì¶ –ü–µ—Ä–µ–Ω–µ—Å—Ç–∏ –Ω–∞–π–¥–µ–Ω–Ω–æ–µ",
        Desc = "–ü–µ—Ä–µ–Ω–µ—Å—Ç–∏ –ø—Ä–µ–¥–º–µ—Ç—ã –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é",
        Callback = function()
            if TextBoxText and TextBoxText ~= "" then
                BringItemsByName(TextBoxText)
            end
        end
    })
end

