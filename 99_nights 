-- 99 Nights in the forest - by Iliankytb
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local MarketplaceService = game:GetService("MarketplaceService")

-- –ó–∞–≥—Ä—É–∂–∞–µ–º –±–∏–±–ª–∏–æ—Ç–µ–∫—É Saul Hub
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/SaulGoodmanScripter/SAUL_HUB/refs/heads/main/Library%20.lua"))()
local Window = Library:Window({
    Title = "99 Nights in the Forest",
    Desc = "by Iliankytb | V.0.33",
    Icon = 92155476988079,
    Theme = "Dark",
    Config = {
        Keybind = Enum.KeyCode.K,
        Size = UDim2.new(0, 550, 0, 350)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "–ó–∞–∫—Ä—ã—Ç—å"
    }
})

-- –¢–≤–æ–∏ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
local V = "V.0.33"
local infoGameName = MarketplaceService:GetProductInfo(game.GameId)
local ActiveEspItems, ActiveEspEnemy, ActiveEspChildren, ActiveEspPeltTrader = false, false, false, false
local ActiveNoclip, ActivateInfiniteJump, ActiveSpeedBoost, ActivateFly = false, false, false, false
local ActiveNoCooldownPrompt, ActiveNoFog, ActiveAutoChopTree, ActiveKillAura = false, false, false, false
local DistanceForKillAura = 25
local DistanceForAutoChopTree = 25
local ValueSpeed = 16
local OldSpeed = 16
local LimitRangerEsp = 100
local DisableLimitRangerEsp = false
local LineESPEnabled = false
local iyflyspeed = 1
local DoingTheThings = false
local OldCFrameHere = nil
local TextBoxText = ""
local isInTheMap = "no"
local HowManyItemCanShowUp = 0
local FLYING = false
local IYMouse = LocalPlayer:GetMouse()
local flyKeyDown, flyKeyUp

-- –¢–≤–æ—è —Ñ—É–Ω–∫—Ü–∏—è DragItem
local function DragItem(Item)
    task.spawn(function()
        for _, tool in pairs(LocalPlayer.Inventory:GetChildren()) do
            if tool:IsA("Model") and tool:GetAttribute("NumberItems") and tool:GetAttribute("Capacity") and tool:GetAttribute("NumberItems") < tool:GetAttribute("Capacity") then
                task.spawn(function()
                    local args = {tool, Item}
                    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestBagStoreItem"):InvokeServer(unpack(args))
                    wait(0.1)
                end)
            end
            wait(0.25)
        end
    end)
end

-- –¢–≤–æ—è —Ñ—É–Ω–∫—Ü–∏—è fly
local function sFLY(vfly)
    repeat wait() until LocalPlayer.Character and LocalPlayer.Character:WaitForChild("HumanoidRootPart") and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    repeat wait() until IYMouse
    if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end

    local T = LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
    local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
    local SPEED = 0

    local function FLY()
        FLYING = true
        local BG = Instance.new('BodyGyro')
        local BV = Instance.new('BodyVelocity')
        BG.P = 9e4
        BG.Parent = T
        BV.Parent = T
        BG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        BG.CFrame = T.CFrame
        BV.Velocity = Vector3.new(0, 0, 0)
        BV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        task.spawn(function()
            repeat wait()
                if not vfly and LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
                    LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = true
                end
                if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
                    SPEED = 50
                elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
                    SPEED = 0
                end
                if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
                    BV.Velocity = ((Workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((Workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - Workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
                    lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
                elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
                    BV.Velocity = ((Workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - Workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
                else
                    BV.Velocity = Vector3.new(0, 0, 0)
                end
                BG.CFrame = Workspace.CurrentCamera.CoordinateFrame
            until not FLYING
            CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
            lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
            SPEED = 0
            BG:Destroy()
            BV:Destroy()
            if LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
                LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
            end
        end)
    end
    
    flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
        if KEY:lower() == 'w' then
            CONTROL.F = iyflyspeed
        elseif KEY:lower() == 's' then
            CONTROL.B = -iyflyspeed
        elseif KEY:lower() == 'a' then
            CONTROL.L = -iyflyspeed
        elseif KEY:lower() == 'd' then 
            CONTROL.R = iyflyspeed
        elseif KEY:lower() == 'e' then
            CONTROL.Q = iyflyspeed*2
        elseif KEY:lower() == 'q' then
            CONTROL.E = -iyflyspeed*2
        end
        pcall(function() Workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
    end)
    
    flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
        if KEY:lower() == 'w' then
            CONTROL.F = 0
        elseif KEY:lower() == 's' then
            CONTROL.B = 0
        elseif KEY:lower() == 'a' then
            CONTROL.L = 0
        elseif KEY:lower() == 'd' then
            CONTROL.R = 0
        elseif KEY:lower() == 'e' then
            CONTROL.Q = 0
        elseif KEY:lower() == 'q' then
            CONTROL.E = 0
        end
    end)
    FLY()
end

local function NOFLY()
    FLYING = false
    if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
    if LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
        LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
    end
    pcall(function() Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
end

-- –í–ö–õ–ê–î–ö–ê INFO
local InfoTab = Window:Tab({Title = "‚ÑπÔ∏è Info", Icon = "info"}) do
    local function getServerInfo()
        local playerCount = #Players:GetPlayers()
        local maxPlayers = Players.MaxPlayers
        local isStudio = RunService:IsStudio()
        
        return {
            PlaceId = game.PlaceId,
            JobId = game.JobId,
            IsStudio = isStudio,
            CurrentPlayers = playerCount,
            MaxPlayers = maxPlayers
        }
    end

    InfoTab:Label({
        Title = "–í–µ—Ä—Å–∏—è —Å–∫—Ä–∏–ø—Ç–∞",
        Desc = V
    })

    local serverInfoLabel = InfoTab:Label({
        Title = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Å–µ—Ä–≤–µ—Ä–µ",
        Desc = "–ó–∞–≥—Ä—É–∑–∫–∞..."
    })

    task.spawn(function()
        while true do
            local updatedInfo = getServerInfo()
            local updatedContent = string.format(
                "üéÆ –ò–≥—Ä–∞: %s\nüìå PlaceId: %s\nüîë JobId: %s\nüß™ Studio: %s\nüë• –ò–≥—Ä–æ–∫–∏: %d/%d",
                infoGameName.Name,
                updatedInfo.PlaceId,
                updatedInfo.JobId,
                tostring(updatedInfo.IsStudio),
                updatedInfo.CurrentPlayers,
                updatedInfo.MaxPlayers
            )
            serverInfoLabel:SetDesc(updatedContent)
            wait(5)
        end
    end)
end

-- –í–ö–õ–ê–î–ö–ê PLAYER
local PlayerTab = Window:Tab({Title = "üë§ Player", Icon = "user"}) do
    PlayerTab:Section({Title = "–î–≤–∏–∂–µ–Ω–∏–µ"})

    -- Noclip
    local noclipConnection
    PlayerTab:Toggle({
        Title = "üëª Noclip",
        Desc = "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã",
        Value = false,
        Callback = function(v)
            ActiveNoclip = v
            local character = LocalPlayer.Character

            if v then
                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end

                if noclipConnection then
                    noclipConnection:Disconnect()
                end

                noclipConnection = RunService.Stepped:Connect(function()
                    if ActiveNoclip and LocalPlayer.Character then
                        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                            end
                        end
                    end
                end)
            else
                if noclipConnection then
                    noclipConnection:Disconnect()
                    noclipConnection = nil
                end

                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = true
                        end
                    end
                end
            end
        end
    })

    -- Infinite Jump
    local jumpConnection
    PlayerTab:Toggle({
        Title = "üèÄ Infinite Jump",
        Desc = "–ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–µ –ø—Ä—ã–∂–∫–∏",
        Value = false,
        Callback = function(v)
            ActivateInfiniteJump = v

            if v then
                jumpConnection = UserInputService.JumpRequest:Connect(function()
                    if ActivateInfiniteJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                        LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end)
            else
                if jumpConnection then
                    jumpConnection:Disconnect()
                    jumpConnection = nil
                end
            end
        end
    })

    PlayerTab:Section({Title = "–°–∫–æ—Ä–æ—Å—Ç—å"})
    
    PlayerTab:Toggle({
        Title = "üöÄ –£—Å–∫–æ—Ä–µ–Ω–∏–µ",
        Desc = "–í–∫–ª—é—á–∏—Ç—å —É–≤–µ–ª–∏—á–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏",
        Value = false,
        Callback = function(v)
            ActiveSpeedBoost = v
            if v then
                OldSpeed = LocalPlayer.Character.Humanoid.WalkSpeed
                LocalPlayer.Character.Humanoid.WalkSpeed = ValueSpeed
            else
                LocalPlayer.Character.Humanoid.WalkSpeed = OldSpeed
            end
        end
    })

    PlayerTab:Slider({
        Title = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å",
        Desc = "–°–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏—è",
        Min = 16,
        Max = 100,
        Rounding = 0,
        Value = 16,
        Callback = function(val)
            ValueSpeed = val
            if ActiveSpeedBoost and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.WalkSpeed = val
            end
        end
    })

    PlayerTab:Section({Title = "–ü–æ–ª—ë—Ç"})
    
    PlayerTab:Toggle({
        Title = "üïäÔ∏è Fly",
        Desc = "–õ–µ—Ç–∞—Ç—å (F –¥–ª—è –≤–∫–ª/–≤—ã–∫–ª)",
        Value = false,
        Callback = function(v)
            ActivateFly = v
            if not FLYING and ActivateFly then
                NOFLY()
                wait()
                sFLY()
            elseif FLYING and not ActivateFly then
                NOFLY()
            end
        end
    })

    PlayerTab:Slider({
        Title = "üïäÔ∏è –°–∫–æ—Ä–æ—Å—Ç—å –ø–æ–ª–µ—Ç–∞",
        Desc = "–†–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è 1-5",
        Min = 1,
        Max = 10,
        Rounding = 1,
        Value = 1,
        Callback = function(val)
            iyflyspeed = val
        end
    })

    PlayerTab:Section({Title = "–ü—Ä–æ—á–µ–µ"})

    PlayerTab:Toggle({
        Title = "üå´Ô∏è No Fog",
        Desc = "–£–±—Ä–∞—Ç—å —Ç—É–º–∞–Ω",
        Value = false,
        Callback = function(v)
            ActiveNoFog = v
            if v then
                for _, part in pairs(Workspace.Map.Boundaries:GetChildren()) do 
                    if part:IsA("Part") then
                        part:Destroy()
                    end
                end
            end
        end
    })

    PlayerTab:Toggle({
        Title = "‚ö° Instant Prompt",
        Desc = "–ú–≥–Ω–æ–≤–µ–Ω–Ω–æ–µ –≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏–µ",
        Value = false,
        Callback = function(v)
            ActiveNoCooldownPrompt = v
            if v then
                for _, Assets in pairs(Workspace:GetDescendants()) do  
                    if Assets:IsA("ProximityPrompt") and Assets.HoldDuration ~= 0 then 
                        Assets:SetAttribute("HoldDurationOld", Assets.HoldDuration)
                        Assets.HoldDuration = 0
                    end 
                end  
            else 
                for _, Assets in pairs(Workspace:GetDescendants()) do  
                    if Assets:IsA("ProximityPrompt") and Assets:GetAttribute("HoldDurationOld") and Assets:GetAttribute("HoldDurationOld") ~= 0 then 
                        Assets.HoldDuration = Assets:GetAttribute("HoldDurationOld")
                    end 
                end  
            end
        end
    })

    PlayerTab:Button({
        Title = "üî• TP to Campfire",
        Desc = "–¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –∫–æ—Å—Ç—Ä—É",
        Callback = function()
            if LocalPlayer.Character then
                LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = Workspace.Map.Campground.MainFire.PrimaryPart.CFrame + Vector3.new(0,10,0)
            end
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê ESP
local EspTab = Window:Tab({Title = "üëÅÔ∏è ESP", Icon = "eye"}) do
    EspTab:Section({Title = "–¢–∏–ø—ã ESP"})

    -- ESP Items
    EspTab:Toggle({
        Title = "üì¶ ESP Items",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveEspItems = v
            if v then
                for _, Obj in pairs(Workspace.Items:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(255,255,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(255,255,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Items:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Enemy
    EspTab:Toggle({
        Title = "üëπ ESP Enemy",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –≤—Ä–∞–≥–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveEspEnemy = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and not (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4" or Obj.Name == "Pelt Trader") and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(255,0,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(255,0,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Children
    EspTab:Toggle({
        Title = "üë∂ ESP Children",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –¥–µ—Ç–µ–π",
        Value = false,
        Callback = function(v)
            ActiveEspChildren = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4") and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(0,255,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(0,255,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Pelt Trader
    EspTab:Toggle({
        Title = "ü¶ä ESP Pelt Trader",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ —Ç–æ—Ä–≥–æ–≤—Ü–∞",
        Value = false,
        Callback = function(v)
            ActiveEspPeltTrader = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and Obj.Name == "Pelt Trader" and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(0,255,255)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(0,255,255)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    EspTab:Section({Title = "–ù–∞—Å—Ç—Ä–æ–π–∫–∏ ESP"})

    EspTab:Toggle({
        Title = "üìè –ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –¥–∏—Å—Ç–∞–Ω—Ü–∏—é",
        Desc = "–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ –æ–±—ä–µ–∫—Ç–æ–≤",
        Value = false,
        Callback = function(v)
            -- –¢–≤–æ—è –ª–æ–≥–∏–∫–∞ –¥–ª—è Distance ESP
        end
    })

    EspTab:Toggle({
        Title = "üìê –õ–∏–Ω–∏–∏ ESP",
        Desc = "–õ–∏–Ω–∏–∏ –æ—Ç –∫–∞–º–µ—Ä—ã –∫ –æ–±—ä–µ–∫—Ç–∞–º",
        Value = false,
        Callback = function(v)
            LineESPEnabled = v
        end
    })

    EspTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è ESP",
        Desc = "–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –¥–∏—Å—Ç–∞–Ω—Ü–∏—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 100,
        Callback = function(val)
            LimitRangerEsp = val
        end
    })

    EspTab:Toggle({
        Title = "‚ôæÔ∏è –ë–µ–∑ –ª–∏–º–∏—Ç–∞ –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏",
        Desc = "–û—Ç–∫–ª—é—á–∏—Ç—å –ª–∏–º–∏—Ç –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏",
        Value = false,
        Callback = function(v)
            DisableLimitRangerEsp = v
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê GAME
local GameTab = Window:Tab({Title = "üéÆ Game", Icon = "gamepad-2"}) do
    GameTab:Section({Title = "–ê–≤—Ç–æ–º–∞—Ç–∏–∑–∞—Ü–∏—è"})

    GameTab:Label({
        Title = "–ü—Ä–∏–º–µ—á–∞–Ω–∏–µ",
        Desc = "–î–ª—è Auto Chop Tree –∏ Kill Aura –Ω—É–∂–µ–Ω —Ç–æ–ø–æ—Ä!"
    })

    -- Kill Aura
    GameTab:Toggle({
        Title = "üó°Ô∏è Kill Aura",
        Desc = "–ê–≤—Ç–æ–∞—Ç–∞–∫–∞ –≤—Ä–∞–≥–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveKillAura = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Kill Aura",
        Desc = "–†–∞–¥–∏—É—Å –∞—Ç–∞–∫–∏",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
            DistanceForKillAura = val
        end
    })

    -- Auto Chop Tree
    GameTab:Toggle({
        Title = "üå≥ Auto Chop Tree",
        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —Ä—É–±–∫–∞ –¥–µ—Ä–µ–≤—å–µ–≤",
        Value = false,
        Callback = function(v)
            ActiveAutoChopTree = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Auto Chop",
        Desc = "–†–∞–¥–∏—É—Å —Ä—É–±–∫–∏ –¥–µ—Ä–µ–≤—å–µ–≤",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
            DistanceForAutoChopTree = val
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê BRING ITEMS
local BringItemTab = Window:Tab({Title = "üì¶ Bring Items", Icon = "package"}) do
    BringItemTab:Section({Title = "–°–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    local isCollecting = false
    local collectionPaused = false
    local spawnPosition = nil -- –ì–¥–µ —Å–ø–∞–≤–Ω–∏—Ç—å –ø—Ä–µ–¥–º–µ—Ç—ã

    -- –§—É–Ω–∫—Ü–∏—è –ø–æ–ª—É—á–µ–Ω–∏—è –ø—Ä–µ–¥–º–µ—Ç–∞ (—Å–ø–∞–≤–Ω–∏—Ç –µ–≥–æ —Ä—è–¥–æ–º —Å –∏–≥—Ä–æ–∫–æ–º)
    local function TakeItem(item)
        if not item or not item.PrimaryPart then 
            return false 
        end
        
        -- –ñ–¥–µ–º —Å—Ç–∞–±–∏–ª–∏–∑–∞—Ü–∏–∏
        task.wait(0.3)
        
        local success = false
        
        -- 1. –ò–¥–µ–º –∫ –ø—Ä–µ–¥–º–µ—Ç—É (–µ—Å–ª–∏ –¥–∞–ª–µ–∫–æ)
        local character = LocalPlayer.Character
        if character then
            local hrp = character:WaitForChild("HumanoidRootPart")
            local distance = (hrp.Position - item.PrimaryPart.Position).Magnitude
            
            if distance > 10 then
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –±–ª–∏–∂–µ
                hrp.CFrame = item.PrimaryPart.CFrame + Vector3.new(0, 3, 0)
                task.wait(0.5)
            end
        end
        
        -- 2. –ü—Ä–æ–±—É–µ–º –≤–∑—è—Ç—å –ø—Ä–µ–¥–º–µ—Ç —Ä–∞–∑–Ω—ã–º–∏ —Å–ø–æ—Å–æ–±–∞–º–∏
        for attempt = 1, 5 do
            -- –°–ø–æ—Å–æ–± 1: –ß–µ—Ä–µ–∑ RemoteEvent (–µ—Å–ª–∏ –µ—Å—Ç—å)
            pcall(function()
                game:GetService("ReplicatedStorage").RemoteEvents.RequestBagStoreItem:FireServer(item)
            end)
            
            -- –°–ø–æ—Å–æ–± 2: –ß–µ—Ä–µ–∑ –¥—Ä—É–≥–æ–π RemoteEvent
            pcall(function()
                game:GetService("ReplicatedStorage").RemoteEvents.CollectItem:FireServer(item)
            end)
            
            -- –°–ø–æ—Å–æ–± 3: –ü—Ä–æ—Å—Ç–æ –∫–ª–∏–∫–∞–µ–º –Ω–∞ –ø—Ä–µ–¥–º–µ—Ç
            pcall(function()
                item:SetPrimaryPartCFrame(Workspace.CurrentCamera.CFrame * CFrame.new(0, 0, -5))
            end)
            
            -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –≤–∑—è–ª—Å—è –ª–∏ –ø—Ä–µ–¥–º–µ—Ç
            if not item or not item.Parent or item.Parent ~= Workspace.Items then
                success = true
                break
            end
            
            task.wait(0.2)
        end
        
        return success
    end

    -- –§—É–Ω–∫—Ü–∏—è —Å–ø–∞–≤–Ω–∞ –ø—Ä–µ–¥–º–µ—Ç–∞ —Ä—è–¥–æ–º —Å –∏–≥—Ä–æ–∫–æ–º
    local function SpawnItemNearPlayer(itemName)
        local character = LocalPlayer.Character
        if not character then return false end
        
        local hrp = character:WaitForChild("HumanoidRootPart")
        
        -- –ú–µ—Å—Ç–æ –¥–ª—è —Å–ø–∞–≤–Ω–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤
        if not spawnPosition then
            spawnPosition = hrp.Position + Vector3.new(0, 0, 5) -- –°–ø–µ—Ä–µ–¥–∏ –æ—Ç –∏–≥—Ä–æ–∫–∞
        end
        
        -- –ü—ã—Ç–∞–µ–º—Å—è —Å–ø–∞–≤–Ω–∏—Ç—å –ø—Ä–µ–¥–º–µ—Ç
        for _, item in pairs(Workspace.Items:GetChildren()) do
            if item.Name == itemName and item:IsA("Model") and item.PrimaryPart then
                -- –ö–ª–æ–Ω–∏—Ä—É–µ–º –ø—Ä–µ–¥–º–µ—Ç
                local clone = item:Clone()
                clone.PrimaryPart = clone:FindFirstChild("Handle") or clone.PrimaryPart
                
                -- –°—Ç–∞–≤–∏–º —Ä—è–¥–æ–º —Å –∏–≥—Ä–æ–∫–æ–º
                clone:SetPrimaryPartCFrame(CFrame.new(spawnPosition))
                clone.Parent = Workspace.Items
                
                -- –°–¥–≤–∏–≥–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –¥–ª—è —Å–ª–µ–¥—É—é—â–µ–≥–æ –ø—Ä–µ–¥–º–µ—Ç–∞
                spawnPosition = spawnPosition + Vector3.new(2, 0, 0)
                
                return true
            end
        end
        
        return false
    end

    -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞
    local function CollectAndSpawnItems(itemName)
        if isCollecting then 
            Window:Notify({
                Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                Desc = "–£–∂–µ –≤—ã–ø–æ–ª–Ω—è–µ—Ç—Å—è —Å–±–æ—Ä!",
                Time = 3
            })
            return 0
        end
        
        isCollecting = true
        collectionPaused = false
        
        local character = LocalPlayer.Character
        if not character then 
            isCollecting = false
            return 0 
        end
        
        local hrp = character:WaitForChild("HumanoidRootPart")
        local originalPosition = hrp.CFrame
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –ø–æ–∑–∏—Ü–∏—é –¥–ª—è —Å–ø–∞–≤–Ω–∞
        spawnPosition = hrp.Position + hrp.CFrame.LookVector * 5 -- 5 –º–µ—Ç—Ä–æ–≤ –≤–ø–µ—Ä–µ–¥
        spawnPosition = Vector3.new(spawnPosition.X, hrp.Position.Y, spawnPosition.Z) -- –¢–∞ –∂–µ –≤—ã—Å–æ—Ç–∞
        
        -- –ù–∞—Ö–æ–¥–∏–º –≤—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã —ç—Ç–æ–≥–æ —Ç–∏–ø–∞
        local items = {}
        for _, item in pairs(Workspace.Items:GetChildren()) do
            if item.Name == itemName and item:IsA("Model") and item.PrimaryPart then
                table.insert(items, {
                    Object = item,
                    Position = item.PrimaryPart.Position,
                    Distance = (hrp.Position - item.PrimaryPart.Position).Magnitude
                })
            end
        end
        
        if #items == 0 then
            Window:Notify({
                Title = "‚ö†Ô∏è –ü—Ä–µ–¥–º–µ—Ç—ã –Ω–µ –Ω–∞–π–¥–µ–Ω—ã",
                Desc = string.format("–ù–µ –Ω–∞–π–¥–µ–Ω–æ –ø—Ä–µ–¥–º–µ—Ç–æ–≤: %s", itemName),
                Time = 3
            })
            isCollecting = false
            return 0
        end
        
        -- –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—é
        table.sort(items, function(a, b)
            return a.Distance < b.Distance
        end)
        
        local collected = 0
        local total = #items
        
        -- –ü—Ä–æ—Ü–µ—Å—Å —Å–±–æ—Ä–∞
        for i, itemData in ipairs(items) do
            if collectionPaused then break end
            
            local item = itemData.Object
            
            -- –ü—Ä–æ–≥—Ä–µ—Å—Å
            if i % 3 == 0 or i == 1 or i == total then
                Window:Notify({
                    Title = "üì¶ –°–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Desc = string.format("[%d/%d] %s", i, total, itemName),
                    Time = 2
                })
            end
            
            -- 1. –ò–¥–µ–º –∫ –ø—Ä–µ–¥–º–µ—Ç—É
            hrp.CFrame = CFrame.new(item.PrimaryPart.Position + Vector3.new(0, 3, 0))
            task.wait(0.5)
            
            -- 2. –°–º–æ—Ç—Ä–∏–º –Ω–∞ –ø—Ä–µ–¥–º–µ—Ç
            local lookAt = CFrame.lookAt(hrp.Position, item.PrimaryPart.Position)
            hrp.CFrame = lookAt
            task.wait(0.2)
            
            -- 3. –ü—ã—Ç–∞–µ–º—Å—è –≤–∑—è—Ç—å –ø—Ä–µ–¥–º–µ—Ç
            local success = TakeItem(item)
            
            if success then
                collected = collected + 1
                
                -- 4. –°–ø–∞–≤–Ω–∏–º –ø—Ä–µ–¥–º–µ—Ç —Ä—è–¥–æ–º —Å —Å–æ–±–æ–π
                task.wait(0.3)
                
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –æ–±—Ä–∞—Ç–Ω–æ –∫ –º–µ—Å—Ç—É —Å–ø–∞–≤–Ω–∞
                hrp.CFrame = CFrame.new(spawnPosition - Vector3.new(0, 3, 0))
                task.wait(0.3)
                
                -- –ü—ã—Ç–∞–µ–º—Å—è —Å–ø–∞–≤–Ω–∏—Ç—å –ø—Ä–µ–¥–º–µ—Ç
                SpawnItemNearPlayer(itemName)
                
                -- –°–¥–≤–∏–≥–∞–µ–º –ø–æ–∑–∏—Ü–∏—é —Å–ø–∞–≤–Ω–∞
                spawnPosition = spawnPosition + Vector3.new(0, 0, -3) -- –ù–∞–∑–∞–¥ –¥–ª—è —Å–ª–µ–¥—É—é—â–µ–≥–æ
            end
            
            -- –ü–∞—É–∑–∞ –º–µ–∂–¥—É –ø—Ä–µ–¥–º–µ—Ç–∞–º–∏
            if i < total and not collectionPaused then
                task.wait(0.4)
            end
        end
        
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è
        task.wait(0.5)
        hrp.CFrame = originalPosition
        
        isCollecting = false
        
        Window:Notify({
            Title = "‚úÖ –°–±–æ—Ä –∑–∞–≤–µ—Ä—à–µ–Ω",
            Desc = string.format("–°–æ–±—Ä–∞–Ω–æ %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤: %s", collected, itemName),
            Time = 5
        })
        
        return collected
    end

    -- –§—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –í–°–ï–• –ø—Ä–µ–¥–º–µ—Ç–æ–≤
    local function CollectAllItems()
        if isCollecting then return 0 end
        
        isCollecting = true
        
        local character = LocalPlayer.Character
        if not character then 
            isCollecting = false
            return 0 
        end
        
        local hrp = character:WaitForChild("HumanoidRootPart")
        local originalPosition = hrp.CFrame
        
        -- –ü–æ–∑–∏—Ü–∏—è –¥–ª—è —Å–ø–∞–≤–Ω–∞
        spawnPosition = hrp.Position + hrp.CFrame.LookVector * 8
        
        -- –°–æ–±–∏—Ä–∞–µ–º –í–°–ï –ø—Ä–µ–¥–º–µ—Ç—ã
        local allItems = {}
        for _, item in pairs(Workspace.Items:GetChildren()) do
            if item:IsA("Model") and item.PrimaryPart then
                table.insert(allItems, {
                    Object = item,
                    Name = item.Name,
                    Position = item.PrimaryPart.Position,
                    Distance = (hrp.Position - item.PrimaryPart.Position).Magnitude
                })
            end
        end
        
        if #allItems == 0 then
            Window:Notify({
                Title = "‚ö†Ô∏è –ù–µ—Ç –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                Desc = "–ù–∞ –∫–∞—Ä—Ç–µ –Ω–µ—Ç –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                Time = 3
            })
            isCollecting = false
            return 0
        end
        
        -- –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞
        table.sort(allItems, function(a, b)
            return a.Distance < b.Distance
        end)
        
        local collected = 0
        local total = #allItems
        
        -- –ü—Ä–æ—Ü–µ—Å—Å
        for i, itemData in ipairs(allItems) do
            if collectionPaused then break end
            
            local item = itemData.Object
            
            -- –ü—Ä–æ–≥—Ä–µ—Å—Å
            if i % 5 == 0 then
                Window:Notify({
                    Title = "üì¶ –°–±–æ—Ä –í–°–ï–• –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Desc = string.format("[%d/%d] –í—Å–µ–≥–æ: %d", i, total, collected),
                    Time = 2
                })
            end
            
            -- –ò–¥–µ–º –∫ –ø—Ä–µ–¥–º–µ—Ç—É
            hrp.CFrame = CFrame.new(item.PrimaryPart.Position + Vector3.new(0, 3, 0))
            task.wait(0.4)
            
            -- –ü—ã—Ç–∞–µ–º—Å—è –≤–∑—è—Ç—å
            local success = TakeItem(item)
            
            if success then
                collected = collected + 1
                
                -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –∫ –º–µ—Å—Ç—É —Å–ø–∞–≤–Ω–∞
                task.wait(0.3)
                hrp.CFrame = CFrame.new(spawnPosition - Vector3.new(0, 3, 0))
                task.wait(0.3)
                
                -- –°–ø–∞–≤–Ω–∏–º –ø—Ä–µ–¥–º–µ—Ç
                pcall(function()
                    local clone = item:Clone()
                    clone:SetPrimaryPartCFrame(CFrame.new(spawnPosition))
                    clone.Parent = Workspace.Items
                    
                    -- –°–¥–≤–∏–≥–∞–µ–º –ø–æ–∑–∏—Ü–∏—é
                    spawnPosition = spawnPosition + Vector3.new(3, 0, 0)
                })
            end
            
            if i < total and not collectionPaused then
                task.wait(0.3)
            end
        end
        
        -- –í–æ–∑–≤—Ä–∞—Ç
        task.wait(0.5)
        hrp.CFrame = originalPosition
        
        isCollecting = false
        
        Window:Notify({
            Title = "üéâ –í–°–ï –ø—Ä–µ–¥–º–µ—Ç—ã —Å–æ–±—Ä–∞–Ω—ã!",
            Desc = string.format("–°–æ–±—Ä–∞–Ω–æ %d –∏–∑ %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤", collected, total),
            Time = 6
        })
        
        return collected
    end

    -- –ë–´–°–¢–†–´–ô –î–û–°–¢–£–ü
    BringItemTab:Section({Title = "üöÄ –ë—ã—Å—Ç—Ä—ã–π –¥–æ—Å—Ç—É–ø"})

    local quickItems = {
        {"üì¶ –í–°–ï –ø—Ä–µ–¥–º–µ—Ç—ã", CollectAllItems},
        {"ü™µ –í—Å–µ –±—Ä–µ–≤–Ω–∞", function() CollectAndSpawnItems("Log") end},
        {"‚ö´Ô∏è –í–µ—Å—å —É–≥–æ–ª—å", function() CollectAndSpawnItems("Coal") end},
        {"‚õΩÔ∏è –í—Å–µ –∫–∞–Ω–∏—Å—Ç—Ä—ã", function() CollectAndSpawnItems("Fuel Canister") end},
        {"ü•ï –í—Å–µ –º–æ—Ä–∫–æ–≤–∫–∏", function() CollectAndSpawnItems("Carrot") end},
        {"ü©π –í—Å–µ –±–∏–Ω—Ç—ã", function() CollectAndSpawnItems("Bandage") end},
        {"üíä –í—Å–µ –∞–ø—Ç–µ—á–∫–∏", function() CollectAndSpawnItems("MedKit") end}
    }

    for _, item in ipairs(quickItems) do
        BringItemTab:Button({
            Title = item[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å –∏ –ø—Ä–∏–Ω–µ—Å—Ç–∏",
            Callback = item[2]
        })
    end

    -- –ú–ê–¢–ï–†–ò–ê–õ–´ –ò –†–ï–°–£–†–°–´
    BringItemTab:Section({Title = "üõ†Ô∏è –†–µ—Å—É—Ä—Å—ã"})

    local resources = {
        {"üî© –õ–∏—Å—Ç—ã –º–µ—Ç–∞–ª–ª–∞", "Sheet Metal"},
        {"üß≤ –ë–æ–ª—Ç—ã", "Bolt"},
        {"üõû –®–∏–Ω—ã", "Tyre"},
        {"üåÄ –í–µ–Ω—Ç–∏–ª—è—Ç–æ—Ä—ã", "Broken Fan"},
        {"üìª –†–∞–¥–∏–æ", "Old Radio"},
        {"üß∞ –°–µ–º–µ–Ω–∞", "Seed Box"},
        {"ü™ë –°—Ç—É–ª—å—è", "Chair"},
        {"üç∞ –¢–æ—Ä—Ç—ã", "Cake"},
        {"ü•™ –°—ç–Ω–¥–≤–∏—á–∏", "Meat? Sandwich"}
    }

    for _, res in ipairs(resources) do
        BringItemTab:Button({
            Title = res[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å " .. res[2],
            Callback = function() CollectAndSpawnItems(res[2]) end
        })
    end

    -- –ì–†–£–ü–ü–´ –ü–†–ï–î–ú–ï–¢–û–í
    BringItemTab:Section({Title = "üìö –ì—Ä—É–ø–ø—ã"})

    BringItemTab:Button({
        Title = "üõ¢Ô∏è –í—Å–µ —Ç–æ–ø–ª–∏–≤–æ",
        Desc = "–ë—Ä–µ–≤–Ω–∞, —É–≥–æ–ª—å, –∫–∞–Ω–∏—Å—Ç—Ä—ã, –±–æ—á–∫–∏",
        Callback = function()
            local fuels = {"Log", "Coal", "Fuel Canister", "Oil Barrel"}
            local totalCollected = 0
            
            for _, fuelType in ipairs(fuels) do
                Window:Notify({
                    Title = "üõ¢Ô∏è –°–±–æ—Ä —Ç–æ–ø–ª–∏–≤–∞",
                    Desc = "–°–æ–±–∏—Ä–∞–µ–º: " .. fuelType,
                    Time = 2
                })
                
                totalCollected = totalCollected + CollectAndSpawnItems(fuelType)
                task.wait(1)
            end
            
            Window:Notify({
                Title = "‚úÖ –í—Å–µ —Ç–æ–ø–ª–∏–≤–æ —Å–æ–±—Ä–∞–Ω–æ",
                Desc = string.format("–í—Å–µ–≥–æ —Å–æ–±—Ä–∞–Ω–æ: %d –µ–¥–∏–Ω–∏—Ü —Ç–æ–ø–ª–∏–≤–∞", totalCollected),
                Time = 4
            })
        end
    })

    BringItemTab:Button({
        Title = "üî´ –í—Å–µ –ø–∞—Ç—Ä–æ–Ω—ã",
        Desc = "–ü–∞—Ç—Ä–æ–Ω—ã –¥–ª—è –≤–∏–Ω—Ç–æ–≤–∫–∏ –∏ —Ä–µ–≤–æ–ª—å–≤–µ—Ä–∞",
        Callback = function()
            local ammos = {"Rifle Ammo", "Revolver Ammo"}
            local total = 0
            
            for _, ammo in ipairs(ammos) do
                total = total + CollectAndSpawnItems(ammo)
                task.wait(0.5)
            end
        end
    })

    -- –ü–û–ò–°–ö
    BringItemTab:Section({Title = "üîç –ü–æ–∏—Å–∫ –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    local currentSearch = ""
    local searchInfo = BringItemTab:Label({
        Title = "–ü–æ–∏—Å–∫ –ø–æ –∏–º–µ–Ω–∏",
        Desc = "–í–≤–µ–¥–∏—Ç–µ —Ç–æ—á–Ω–æ–µ –Ω–∞–∑–≤–∞–Ω–∏–µ"
    })

    BringItemTab:Textbox({
        Title = "–ù–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞",
        Desc = "–°–∫–æ–ø–∏—Ä—É–π—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –∏–∑ ESP",
        Placeholder = "–ù–∞–ø—Ä–∏–º–µ—Ä: Broken Microwave",
        Value = "",
        ClearText = false,
        Callback = function(text)
            currentSearch = text
            if text and text ~= "" then
                local count = 0
                for _, item in pairs(Workspace.Items:GetChildren()) do
                    if item.Name == text and item:IsA("Model") then
                        count = count + 1
                    end
                end
                searchInfo:SetDesc(string.format("–ù–∞–π–¥–µ–Ω–æ: %d | %s", count, text))
            end
        end
    })

    BringItemTab:Button({
        Title = "üì¶ –°–æ–±—Ä–∞—Ç—å –Ω–∞–π–¥–µ–Ω–Ω–æ–µ",
        Desc = "–°–æ–±—Ä–∞—Ç—å –ø—Ä–µ–¥–º–µ—Ç—ã –ø–æ –ø–æ–∏—Å–∫—É",
        Callback = function()
            if currentSearch and currentSearch ~= "" then
                CollectAndSpawnItems(currentSearch)
            else
                Window:Notify({
                    Title = "üîç –ù–µ—Ç –ø–æ–∏—Å–∫–∞",
                    Desc = "–°–Ω–∞—á–∞–ª–∞ –≤–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ",
                    Time = 3
                })
            end
        end
    })

    -- –£–ü–†–ê–í–õ–ï–ù–ò–ï
    BringItemTab:Section({Title = "‚öôÔ∏è –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ"})

    BringItemTab:Toggle({
        Title = "‚è∏Ô∏è –ü–∞—É–∑–∞ –º–µ–∂–¥—É –ø—Ä–µ–¥–º–µ—Ç–∞–º–∏",
        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –ø–∞—É–∑–∞",
        Value = true,
        Callback = function(v)
            -- –†–µ–≥—É–ª–∏—Ä—É–µ—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ –ø–∞—É–∑—ã
        end
    })

    BringItemTab:Button({
        Title = "‚è∏Ô∏è –ü–∞—É–∑–∞ —Å–±–æ—Ä–∞",
        Desc = "–í—Ä–µ–º–µ–Ω–Ω–æ –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Å–±–æ—Ä",
        Callback = function()
            collectionPaused = true
            Window:Notify({
                Title = "‚è∏Ô∏è –°–±–æ—Ä –Ω–∞ –ø–∞—É–∑–µ",
                Desc = "–ù–∞–∂–º–∏—Ç–µ –µ—â–µ —Ä–∞–∑ –¥–ª—è –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏—è",
                Time = 3
            })
        end
    })

    BringItemTab:Button({
        Title = "‚ñ∂Ô∏è –ü—Ä–æ–¥–æ–ª–∂–∏—Ç—å —Å–±–æ—Ä",
        Desc = "–ü—Ä–æ–¥–æ–ª–∂–∏—Ç—å –ø–æ—Å–ª–µ –ø–∞—É–∑—ã",
        Callback = function()
            collectionPaused = false
            Window:Notify({
                Title = "‚ñ∂Ô∏è –°–±–æ—Ä –ø—Ä–æ–¥–æ–ª–∂–µ–Ω",
                Desc = "–ü—Ä–æ—Ü–µ—Å—Å —Å–±–æ—Ä–∞ –≤–æ–∑–æ–±–Ω–æ–≤–ª–µ–Ω",
                Time = 2
            })
        end
    })

    BringItemTab:Button({
        Title = "üõë –≠–∫—Å—Ç—Ä–µ–Ω–Ω–∞—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∞",
        Desc = "–ù–µ–º–µ–¥–ª–µ–Ω–Ω–æ –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—å –≤—Å—ë",
        Callback = function()
            isCollecting = false
            collectionPaused = true
            
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∏–≥—Ä–æ–∫–∞
            local character = LocalPlayer.Character
            if character then
                local hrp = character:WaitForChild("HumanoidRootPart")
                pcall(function()
                    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –Ω–∞ –±–µ–∑–æ–ø–∞—Å–Ω–æ–µ –º–µ—Å—Ç–æ
                    hrp.CFrame = CFrame.new(0, 50, 0)
                end)
            end
            
            Window:Notify({
                Title = "üõë –í–°–Å –û–°–¢–ê–ù–û–í–õ–ï–ù–û",
                Desc = "–°–±–æ—Ä –ø–æ–ª–Ω–æ—Å—Ç—å—é –ø—Ä–µ—Ä–≤–∞–Ω",
                Time = 5
            })
        end
    })

    -- –°–¢–ê–¢–£–° –ò –ò–ù–§–û
    BringItemTab:Section({Title = "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"})

    local statusDisplay = BringItemTab:Label({
        Title = "–°—Ç–∞—Ç—É—Å —Å–∏—Å—Ç–µ–º—ã",
        Desc = "–ó–∞–≥—Ä—É–∑–∫–∞..."
    })

    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞
    task.spawn(function()
        while true do
            if isCollecting then
                if collectionPaused then
                    statusDisplay:SetDesc("‚è∏Ô∏è –°–±–æ—Ä –Ω–∞ –ø–∞—É–∑–µ")
                else
                    statusDisplay:SetDesc("üîÑ –ò–¥–µ—Ç —Å–±–æ—Ä...")
                end
            else
                -- –°—á–∏—Ç–∞–µ–º –ø—Ä–µ–¥–º–µ—Ç—ã
                local itemCount = 0
                local types = {}
                
                for _, item in pairs(Workspace.Items:GetChildren()) do
                    if item:IsA("Model") then
                        itemCount = itemCount + 1
                        types[item.Name] = (types[item.Name] or 0) + 1
                    end
                end
                
                local uniqueTypes = 0
                for _ in pairs(types) do
                    uniqueTypes = uniqueTypes + 1
                end
                
                statusDisplay:SetDesc(string.format("‚úÖ –ì–æ—Ç–æ–≤ | –ü—Ä–µ–¥–º–µ—Ç–æ–≤: %d | –¢–∏–ø–æ–≤: %d", itemCount, uniqueTypes))
            end
            task.wait(3)
        end
    end)

    -- –ö–ù–û–ü–ö–ê –¢–ï–°–¢–ê
    BringItemTab:Button({
        Title = "üß™ –¢–µ—Å—Ç —Å–±–æ—Ä–∞",
        Desc = "–ü—Ä–æ—Ç–µ—Å—Ç–∏—Ä–æ–≤–∞—Ç—å —Å–±–æ—Ä 1 –ø—Ä–µ–¥–º–µ—Ç–∞",
        Callback = function()
            -- –ò—â–µ–º –±–ª–∏–∂–∞–π—à–∏–π –ø—Ä–µ–¥–º–µ—Ç
            local character = LocalPlayer.Character
            if not character then return end
            
            local hrp = character:WaitForChild("HumanoidRootPart")
            local closestItem = nil
            local closestDist = math.huge
            
            for _, item in pairs(Workspace.Items:GetChildren()) do
                if item:IsA("Model") and item.PrimaryPart then
                    local dist = (hrp.Position - item.PrimaryPart.Position).Magnitude
                    if dist < closestDist and dist < 50 then
                        closestDist = dist
                        closestItem = item
                    end
                end
            end
            
            if closestItem then
                Window:Notify({
                    Title = "üß™ –¢–µ—Å—Ç–æ–≤—ã–π —Å–±–æ—Ä",
                    Desc = "–¢–µ—Å—Ç–∏—Ä—É–µ–º: " .. closestItem.Name,
                    Time = 3
                })
                
                -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ –ø—Ä–µ–¥–º–µ—Ç—É
                hrp.CFrame = closestItem.PrimaryPart.CFrame + Vector3.new(0, 3, 0)
                task.wait(0.5)
                
                -- –ü—Ä–æ–±—É–µ–º –≤–∑—è—Ç—å
                local success = TakeItem(closestItem)
                
                if success then
                    Window:Notify({
                        Title = "‚úÖ –¢–µ—Å—Ç —É—Å–ø–µ—à–µ–Ω",
                        Desc = "–ü—Ä–µ–¥–º–µ—Ç –ø–æ–ª—É—á–µ–Ω!",
                        Time = 3
                    })
                else
                    Window:Notify({
                        Title = "‚ùå –¢–µ—Å—Ç –Ω–µ —É–¥–∞–ª—Å—è",
                        Desc = "–ù–µ —É–¥–∞–ª–æ—Å—å –≤–∑—è—Ç—å –ø—Ä–µ–¥–º–µ—Ç",
                        Time = 3
                    })
                end
            else
                Window:Notify({
                    Title = "‚ö†Ô∏è –ù–µ—Ç –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Desc = "–ë–ª–∏–∑–∫–∏—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ",
                    Time = 3
                })
            end
        end
    })
end
