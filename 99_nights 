    })

    -- Kill Aura
    GameTab:Toggle({
        Title = "üó°Ô∏è Kill Aura",
        Desc = "–ê–≤—Ç–æ–∞—Ç–∞–∫–∞ –≤—Ä–∞–≥–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveKillAura = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Kill Aura",
        Desc = "–†–∞–¥–∏—É—Å –∞—Ç–∞–∫–∏",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
