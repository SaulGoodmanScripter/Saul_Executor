-- 99 Nights in the forest - by Iliankytb
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer
local UserInputService = game:GetService("UserInputService")
local MarketplaceService = game:GetService("MarketplaceService")

-- –ó–∞–≥—Ä—É–∂–∞–µ–º –±–∏–±–ª–∏–æ—Ç–µ–∫—É Saul Hub
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/SaulGoodmanScripter/SAUL_HUB/refs/heads/main/Library%20.lua"))()
local Window = Library:Window({
    Title = "99 Nights in the Forest",
    Desc = "by Iliankytb | V.0.33",
    Icon = 92155476988079,
    Theme = "Dark",
    Config = {
        Keybind = Enum.KeyCode.K,
        Size = UDim2.new(0, 550, 0, 350)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "–ó–∞–∫—Ä—ã—Ç—å"
    }
})

-- –¢–≤–æ–∏ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ
local V = "V.0.33"
local infoGameName = MarketplaceService:GetProductInfo(game.GameId)
local ActiveEspItems, ActiveEspEnemy, ActiveEspChildren, ActiveEspPeltTrader = false, false, false, false
local ActiveNoclip, ActivateInfiniteJump, ActiveSpeedBoost, ActivateFly = false, false, false, false
local ActiveNoCooldownPrompt, ActiveNoFog, ActiveAutoChopTree, ActiveKillAura = false, false, false, false
local DistanceForKillAura = 25
local DistanceForAutoChopTree = 25
local ValueSpeed = 16
local OldSpeed = 16
local LimitRangerEsp = 100
local DisableLimitRangerEsp = false
local LineESPEnabled = false
local iyflyspeed = 1
local DoingTheThings = false
local OldCFrameHere = nil
local TextBoxText = ""
local isInTheMap = "no"
local HowManyItemCanShowUp = 0
local FLYING = false
local IYMouse = LocalPlayer:GetMouse()
local flyKeyDown, flyKeyUp

-- –¢–≤–æ—è —Ñ—É–Ω–∫—Ü–∏—è DragItem
local function DragItem(Item)
    task.spawn(function()
        for _, tool in pairs(LocalPlayer.Inventory:GetChildren()) do
            if tool:IsA("Model") and tool:GetAttribute("NumberItems") and tool:GetAttribute("Capacity") and tool:GetAttribute("NumberItems") < tool:GetAttribute("Capacity") then
                task.spawn(function()
                    local args = {tool, Item}
                    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestBagStoreItem"):InvokeServer(unpack(args))
                    wait(0.1)
                end)
            end
            wait(0.25)
        end
    end)
end

-- –¢–≤–æ—è —Ñ—É–Ω–∫—Ü–∏—è fly
local function sFLY(vfly)
    repeat wait() until LocalPlayer.Character and LocalPlayer.Character:WaitForChild("HumanoidRootPart") and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    repeat wait() until IYMouse
    if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end

    local T = LocalPlayer.Character:WaitForChild("HumanoidRootPart")
    local CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
    local lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
    local SPEED = 0

    local function FLY()
        FLYING = true
        local BG = Instance.new('BodyGyro')
        local BV = Instance.new('BodyVelocity')
        BG.P = 9e4
        BG.Parent = T
        BV.Parent = T
        BG.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
        BG.CFrame = T.CFrame
        BV.Velocity = Vector3.new(0, 0, 0)
        BV.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        task.spawn(function()
            repeat wait()
                if not vfly and LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
                    LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = true
                end
                if CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0 then
                    SPEED = 50
                elseif not (CONTROL.L + CONTROL.R ~= 0 or CONTROL.F + CONTROL.B ~= 0 or CONTROL.Q + CONTROL.E ~= 0) and SPEED ~= 0 then
                    SPEED = 0
                end
                if (CONTROL.L + CONTROL.R) ~= 0 or (CONTROL.F + CONTROL.B) ~= 0 or (CONTROL.Q + CONTROL.E) ~= 0 then
                    BV.Velocity = ((Workspace.CurrentCamera.CoordinateFrame.lookVector * (CONTROL.F + CONTROL.B)) + ((Workspace.CurrentCamera.CoordinateFrame * CFrame.new(CONTROL.L + CONTROL.R, (CONTROL.F + CONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - Workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
                    lCONTROL = {F = CONTROL.F, B = CONTROL.B, L = CONTROL.L, R = CONTROL.R}
                elseif (CONTROL.L + CONTROL.R) == 0 and (CONTROL.F + CONTROL.B) == 0 and (CONTROL.Q + CONTROL.E) == 0 and SPEED ~= 0 then
                    BV.Velocity = ((Workspace.CurrentCamera.CoordinateFrame.lookVector * (lCONTROL.F + lCONTROL.B)) + ((Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lCONTROL.L + lCONTROL.R, (lCONTROL.F + lCONTROL.B + CONTROL.Q + CONTROL.E) * 0.2, 0).p) - Workspace.CurrentCamera.CoordinateFrame.p)) * SPEED
                else
                    BV.Velocity = Vector3.new(0, 0, 0)
                end
                BG.CFrame = Workspace.CurrentCamera.CoordinateFrame
            until not FLYING
            CONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
            lCONTROL = {F = 0, B = 0, L = 0, R = 0, Q = 0, E = 0}
            SPEED = 0
            BG:Destroy()
            BV:Destroy()
            if LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
                LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
            end
        end)
    end
    
    flyKeyDown = IYMouse.KeyDown:Connect(function(KEY)
        if KEY:lower() == 'w' then
            CONTROL.F = iyflyspeed
        elseif KEY:lower() == 's' then
            CONTROL.B = -iyflyspeed
        elseif KEY:lower() == 'a' then
            CONTROL.L = -iyflyspeed
        elseif KEY:lower() == 'd' then 
            CONTROL.R = iyflyspeed
        elseif KEY:lower() == 'e' then
            CONTROL.Q = iyflyspeed*2
        elseif KEY:lower() == 'q' then
            CONTROL.E = -iyflyspeed*2
        end
        pcall(function() Workspace.CurrentCamera.CameraType = Enum.CameraType.Track end)
    end)
    
    flyKeyUp = IYMouse.KeyUp:Connect(function(KEY)
        if KEY:lower() == 'w' then
            CONTROL.F = 0
        elseif KEY:lower() == 's' then
            CONTROL.B = 0
        elseif KEY:lower() == 'a' then
            CONTROL.L = 0
        elseif KEY:lower() == 'd' then
            CONTROL.R = 0
        elseif KEY:lower() == 'e' then
            CONTROL.Q = 0
        elseif KEY:lower() == 'q' then
            CONTROL.E = 0
        end
    end)
    FLY()
end

local function NOFLY()
    FLYING = false
    if flyKeyDown or flyKeyUp then flyKeyDown:Disconnect() flyKeyUp:Disconnect() end
    if LocalPlayer.Character:FindFirstChildOfClass('Humanoid') then
        LocalPlayer.Character:FindFirstChildOfClass('Humanoid').PlatformStand = false
    end
    pcall(function() Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom end)
end

-- –í–ö–õ–ê–î–ö–ê INFO
local InfoTab = Window:Tab({Title = "‚ÑπÔ∏è Info", Icon = "info"}) do
    local function getServerInfo()
        local playerCount = #Players:GetPlayers()
        local maxPlayers = Players.MaxPlayers
        local isStudio = RunService:IsStudio()
        
        return {
            PlaceId = game.PlaceId,
            JobId = game.JobId,
            IsStudio = isStudio,
            CurrentPlayers = playerCount,
            MaxPlayers = maxPlayers
        }
    end

    InfoTab:Label({
        Title = "–í–µ—Ä—Å–∏—è —Å–∫—Ä–∏–ø—Ç–∞",
        Desc = V
    })

    local serverInfoLabel = InfoTab:Label({
        Title = "–ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Å–µ—Ä–≤–µ—Ä–µ",
        Desc = "–ó–∞–≥—Ä—É–∑–∫–∞..."
    })

    task.spawn(function()
        while true do
            local updatedInfo = getServerInfo()
            local updatedContent = string.format(
                "üéÆ –ò–≥—Ä–∞: %s\nüìå PlaceId: %s\nüîë JobId: %s\nüß™ Studio: %s\nüë• –ò–≥—Ä–æ–∫–∏: %d/%d",
                infoGameName.Name,
                updatedInfo.PlaceId,
                updatedInfo.JobId,
                tostring(updatedInfo.IsStudio),
                updatedInfo.CurrentPlayers,
                updatedInfo.MaxPlayers
            )
            serverInfoLabel:SetDesc(updatedContent)
            wait(5)
        end
    end)
end

-- –í–ö–õ–ê–î–ö–ê PLAYER
local PlayerTab = Window:Tab({Title = "üë§ Player", Icon = "user"}) do
    PlayerTab:Section({Title = "–î–≤–∏–∂–µ–Ω–∏–µ"})

    -- Noclip
    local noclipConnection
    PlayerTab:Toggle({
        Title = "üëª Noclip",
        Desc = "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã",
        Value = false,
        Callback = function(v)
            ActiveNoclip = v
            local character = LocalPlayer.Character

            if v then
                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end

                if noclipConnection then
                    noclipConnection:Disconnect()
                end

                noclipConnection = RunService.Stepped:Connect(function()
                    if ActiveNoclip and LocalPlayer.Character then
                        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                            end
                        end
                    end
                end)
            else
                if noclipConnection then
                    noclipConnection:Disconnect()
                    noclipConnection = nil
                end

                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = true
                        end
                    end
                end
            end
        end
    })

    -- Infinite Jump
    local jumpConnection
    PlayerTab:Toggle({
        Title = "üèÄ Infinite Jump",
        Desc = "–ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–µ –ø—Ä—ã–∂–∫–∏",
        Value = false,
        Callback = function(v)
            ActivateInfiniteJump = v

            if v then
                jumpConnection = UserInputService.JumpRequest:Connect(function()
                    if ActivateInfiniteJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                        LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    end
                end)
            else
                if jumpConnection then
                    jumpConnection:Disconnect()
                    jumpConnection = nil
                end
            end
        end
    })

    PlayerTab:Section({Title = "–°–∫–æ—Ä–æ—Å—Ç—å"})
    
    PlayerTab:Toggle({
        Title = "üöÄ –£—Å–∫–æ—Ä–µ–Ω–∏–µ",
        Desc = "–í–∫–ª—é—á–∏—Ç—å —É–≤–µ–ª–∏—á–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏",
        Value = false,
        Callback = function(v)
            ActiveSpeedBoost = v
            if v then
                OldSpeed = LocalPlayer.Character.Humanoid.WalkSpeed
                LocalPlayer.Character.Humanoid.WalkSpeed = ValueSpeed
            else
                LocalPlayer.Character.Humanoid.WalkSpeed = OldSpeed
            end
        end
    })

    PlayerTab:Slider({
        Title = "‚ö° –°–∫–æ—Ä–æ—Å—Ç—å",
        Desc = "–°–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏—è",
        Min = 16,
        Max = 100,
        Rounding = 0,
        Value = 16,
        Callback = function(val)
            ValueSpeed = val
            if ActiveSpeedBoost and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.WalkSpeed = val
            end
        end
    })

    PlayerTab:Section({Title = "–ü–æ–ª—ë—Ç"})
    
    PlayerTab:Toggle({
        Title = "üïäÔ∏è Fly",
        Desc = "–õ–µ—Ç–∞—Ç—å (F –¥–ª—è –≤–∫–ª/–≤—ã–∫–ª)",
        Value = false,
        Callback = function(v)
            ActivateFly = v
            if not FLYING and ActivateFly then
                NOFLY()
                wait()
                sFLY()
            elseif FLYING and not ActivateFly then
                NOFLY()
            end
        end
    })

    PlayerTab:Slider({
        Title = "üïäÔ∏è –°–∫–æ—Ä–æ—Å—Ç—å –ø–æ–ª–µ—Ç–∞",
        Desc = "–†–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è 1-5",
        Min = 1,
        Max = 10,
        Rounding = 1,
        Value = 1,
        Callback = function(val)
            iyflyspeed = val
        end
    })

    PlayerTab:Section({Title = "–ü—Ä–æ—á–µ–µ"})

    PlayerTab:Toggle({
        Title = "üå´Ô∏è No Fog",
        Desc = "–£–±—Ä–∞—Ç—å —Ç—É–º–∞–Ω",
        Value = false,
        Callback = function(v)
            ActiveNoFog = v
            if v then
                for _, part in pairs(Workspace.Map.Boundaries:GetChildren()) do 
                    if part:IsA("Part") then
                        part:Destroy()
                    end
                end
            end
        end
    })

    PlayerTab:Toggle({
        Title = "‚ö° Instant Prompt",
        Desc = "–ú–≥–Ω–æ–≤–µ–Ω–Ω–æ–µ –≤–∑–∞–∏–º–æ–¥–µ–π—Å—Ç–≤–∏–µ",
        Value = false,
        Callback = function(v)
            ActiveNoCooldownPrompt = v
            if v then
                for _, Assets in pairs(Workspace:GetDescendants()) do  
                    if Assets:IsA("ProximityPrompt") and Assets.HoldDuration ~= 0 then 
                        Assets:SetAttribute("HoldDurationOld", Assets.HoldDuration)
                        Assets.HoldDuration = 0
                    end 
                end  
            else 
                for _, Assets in pairs(Workspace:GetDescendants()) do  
                    if Assets:IsA("ProximityPrompt") and Assets:GetAttribute("HoldDurationOld") and Assets:GetAttribute("HoldDurationOld") ~= 0 then 
                        Assets.HoldDuration = Assets:GetAttribute("HoldDurationOld")
                    end 
                end  
            end
        end
    })

    PlayerTab:Button({
        Title = "üî• TP to Campfire",
        Desc = "–¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –∫–æ—Å—Ç—Ä—É",
        Callback = function()
            if LocalPlayer.Character then
                LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = Workspace.Map.Campground.MainFire.PrimaryPart.CFrame + Vector3.new(0,10,0)
            end
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê ESP
local EspTab = Window:Tab({Title = "üëÅÔ∏è ESP", Icon = "eye"}) do
    EspTab:Section({Title = "–¢–∏–ø—ã ESP"})

    -- ESP Items
    EspTab:Toggle({
        Title = "üì¶ ESP Items",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveEspItems = v
            if v then
                for _, Obj in pairs(Workspace.Items:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(255,255,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(255,255,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Items:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Enemy
    EspTab:Toggle({
        Title = "üëπ ESP Enemy",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –≤—Ä–∞–≥–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveEspEnemy = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and not (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4" or Obj.Name == "Pelt Trader") and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(255,0,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(255,0,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Children
    EspTab:Toggle({
        Title = "üë∂ ESP Children",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –¥–µ—Ç–µ–π",
        Value = false,
        Callback = function(v)
            ActiveEspChildren = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4") and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(0,255,0)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(0,255,0)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    -- ESP Pelt Trader
    EspTab:Toggle({
        Title = "ü¶ä ESP Pelt Trader",
        Desc = "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ —Ç–æ—Ä–≥–æ–≤—Ü–∞",
        Value = false,
        Callback = function(v)
            ActiveEspPeltTrader = v
            if v then
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj.PrimaryPart and Obj.Name == "Pelt Trader" and not Obj:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "ESP_Highlight"
                        highlight.Adornee = Obj
                        highlight.FillColor = Color3.fromRGB(0,255,255)
                        highlight.FillTransparency = 1
                        highlight.OutlineColor = Color3.fromRGB(0,255,255)
                        highlight.OutlineTransparency = 0
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = Obj
                    end 
                end
            else
                for _, Obj in pairs(Workspace.Characters:GetChildren()) do 
                    if Obj:IsA("Model") and Obj:FindFirstChildOfClass("Highlight") then
                        Obj:FindFirstChildOfClass("Highlight"):Destroy()
                    end 
                end
            end
        end
    })

    EspTab:Section({Title = "–ù–∞—Å—Ç—Ä–æ–π–∫–∏ ESP"})

    EspTab:Toggle({
        Title = "üìè –ü–æ–∫–∞–∑—ã–≤–∞—Ç—å –¥–∏—Å—Ç–∞–Ω—Ü–∏—é",
        Desc = "–ü–æ–∫–∞–∑—ã–≤–∞—Ç—å —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ –æ–±—ä–µ–∫—Ç–æ–≤",
        Value = false,
        Callback = function(v)
            -- –¢–≤–æ—è –ª–æ–≥–∏–∫–∞ –¥–ª—è Distance ESP
        end
    })

    EspTab:Toggle({
        Title = "üìê –õ–∏–Ω–∏–∏ ESP",
        Desc = "–õ–∏–Ω–∏–∏ –æ—Ç –∫–∞–º–µ—Ä—ã –∫ –æ–±—ä–µ–∫—Ç–∞–º",
        Value = false,
        Callback = function(v)
            LineESPEnabled = v
        end
    })

    EspTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è ESP",
        Desc = "–ú–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –¥–∏—Å—Ç–∞–Ω—Ü–∏—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 100,
        Callback = function(val)
            LimitRangerEsp = val
        end
    })

    EspTab:Toggle({
        Title = "‚ôæÔ∏è –ë–µ–∑ –ª–∏–º–∏—Ç–∞ –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏",
        Desc = "–û—Ç–∫–ª—é—á–∏—Ç—å –ª–∏–º–∏—Ç –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏",
        Value = false,
        Callback = function(v)
            DisableLimitRangerEsp = v
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê GAME
local GameTab = Window:Tab({Title = "üéÆ Game", Icon = "gamepad-2"}) do
    GameTab:Section({Title = "–ê–≤—Ç–æ–º–∞—Ç–∏–∑–∞—Ü–∏—è"})

    GameTab:Label({
        Title = "–ü—Ä–∏–º–µ—á–∞–Ω–∏–µ",
        Desc = "–î–ª—è Auto Chop Tree –∏ Kill Aura –Ω—É–∂–µ–Ω —Ç–æ–ø–æ—Ä!"
    })

    -- Kill Aura
    GameTab:Toggle({
        Title = "üó°Ô∏è Kill Aura",
        Desc = "–ê–≤—Ç–æ–∞—Ç–∞–∫–∞ –≤—Ä–∞–≥–æ–≤",
        Value = false,
        Callback = function(v)
            ActiveKillAura = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Kill Aura",
        Desc = "–†–∞–¥–∏—É—Å –∞—Ç–∞–∫–∏",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
            DistanceForKillAura = val
        end
    })

    -- Auto Chop Tree
    GameTab:Toggle({
        Title = "üå≥ Auto Chop Tree",
        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —Ä—É–±–∫–∞ –¥–µ—Ä–µ–≤—å–µ–≤",
        Value = false,
        Callback = function(v)
            ActiveAutoChopTree = v
        end
    })

    GameTab:Slider({
        Title = "üéØ –î–∏—Å—Ç–∞–Ω—Ü–∏—è Auto Chop",
        Desc = "–†–∞–¥–∏—É—Å —Ä—É–±–∫–∏ –¥–µ—Ä–µ–≤—å–µ–≤",
        Min = 25,
        Max = 1000,
        Rounding = 0,
        Value = 25,
        Callback = function(val)
            DistanceForAutoChopTree = val
        end
    })
end

-- –í–ö–õ–ê–î–ö–ê BRING ITEMS
local BringItemTab = Window:Tab({Title = "üì¶ Bring Items", Icon = "package"}) do
    BringItemTab:Section({Title = "–°–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    -- –ì–ª–æ–±–∞–ª—å–Ω—ã–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è —ç—Ç–æ–π –≤–∫–ª–∞–¥–∫–∏
    local TextBoxText = ""
    local SearchResults = {}
    local SelectedItemType = "–í—Å–µ"

    -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤
    local function CollectItems(itemList, itemName)
        if DoingTheThings then 
            Window:Notify({
                Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                Desc = "–£–∂–µ –≤—ã–ø–æ–ª–Ω—è–µ—Ç—Å—è —Å–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤!",
                Time = 3
            })
            return 
        end
        
        DoingTheThings = true
        local startTime = tick()
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –ø–æ–∑–∏—Ü–∏—é
        local character = LocalPlayer.Character
        if not character then 
            DoingTheThings = false
            return 
        end
        
        local hrp = character:WaitForChild("HumanoidRootPart")
        OldCFrameHere = hrp.CFrame
        
        -- –ü–æ–¥–≥–æ—Ç–æ–≤–∫–∞ —Å–ø–∏—Å–∫–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤
        local itemsToCollect = {}
        for _, item in pairs(itemList) do
            if item:IsA("Model") and item.PrimaryPart then
                table.insert(itemsToCollect, {
                    Object = item,
                    Distance = (hrp.Position - item.PrimaryPart.Position).Magnitude
                })
            end
        end
        
        -- –°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—é
        table.sort(itemsToCollect, function(a, b)
            return a.Distance < b.Distance
        end)
        
        local collectedCount = 0
        local failedCount = 0
        
        -- –ü—Ä–æ—Ü–µ—Å—Å —Å–±–æ—Ä–∞
        for i, itemData in ipairs(itemsToCollect) do
            local item = itemData.Object
            
            -- –û–±–Ω–æ–≤–ª—è–µ–º UI –∫–∞–∂–¥—ã–µ 5 –ø—Ä–µ–¥–º–µ—Ç–æ–≤
            if i % 5 == 0 then
                Window:Notify({
                    Title = "üì¶ –°–±–æ—Ä –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Desc = string.format("–ü—Ä–æ–≥—Ä–µ—Å—Å: %d/%d", i, #itemsToCollect),
                    Time = 2
                })
            end
            
            -- –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –ø—Ä–µ–¥–º–µ—Ç—É
            hrp.CFrame = item.PrimaryPart.CFrame + Vector3.new(0, 2, 0)
            wait(0.3)
            
            -- –ü–æ–ø—ã—Ç–∫–∞ —Å–±–æ—Ä–∞
            local success = false
            for attempt = 1, 3 do
                pcall(function()
                    for _, tool in pairs(LocalPlayer.Inventory:GetChildren()) do
                        if tool:IsA("Model") and tool:GetAttribute("NumberItems") and tool:GetAttribute("Capacity") then
                            if tool:GetAttribute("NumberItems") < tool:GetAttribute("Capacity") then
                                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("RequestBagStoreItem"):InvokeServer(tool, item)
                                success = true
                                break
                            end
                        end
                    end
                end)
                
                if success then break end
                wait(0.1)
            end
            
            if success then
                collectedCount = collectedCount + 1
            else
                failedCount = failedCount + 1
            end
            
            -- –ü–∞—É–∑–∞ –º–µ–∂–¥—É –ø—Ä–µ–¥–º–µ—Ç–∞–º–∏
            if i < #itemsToCollect then
                wait(0.4)
            end
        end
        
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º—Å—è –Ω–∞ –º–µ—Å—Ç–æ
        wait(0.5)
        hrp.CFrame = OldCFrameHere
        
        DoingTheThings = false
        
        -- –†–µ–∑—É–ª—å—Ç–∞—Ç
        Window:Notify({
            Title = "‚úÖ –°–±–æ—Ä –∑–∞–≤–µ—Ä—à–µ–Ω",
            Desc = string.format("–°–æ–±—Ä–∞–Ω–æ: %d | –ù–µ —Å–æ–±—Ä–∞–Ω–æ: %d", collectedCount, failedCount),
            Time = 5
        })
    end

    -- –§—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –ø–æ –∏–º–µ–Ω–∏
    local function CollectByName(name)
        local items = {}
        for _, Obj in pairs(Workspace.Items:GetChildren()) do
            if Obj.Name == name and Obj:IsA("Model") and Obj.PrimaryPart then
                table.insert(items, Obj)
            end
        end
        CollectItems(items, name)
    end

    -- –§—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –ø–æ —Ç–∏–ø—É
    local function CollectByType(itemTypes)
        local items = {}
        for _, Obj in pairs(Workspace.Items:GetChildren()) do
            if Obj:IsA("Model") and Obj.PrimaryPart then
                local found = false
                for _, itemType in ipairs(itemTypes) do
                    if Obj.Name == itemType then
                        found = true
                        break
                    end
                end
                if found then
                    table.insert(items, Obj)
                end
            end
        end
        CollectItems(items, "–ì—Ä—É–ø–ø–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤")
    end

    -- –°–µ–∫—Ü–∏—è –±—ã—Å—Ç—Ä–æ–≥–æ –¥–æ—Å—Ç—É–ø–∞
    BringItemTab:Section({Title = "üöÄ –ë—ã—Å—Ç—Ä—ã–π –¥–æ—Å—Ç—É–ø"})

    local quickAccessItems = {
        {"üì¶ –í—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã", function()
            local items = {}
            for _, Obj in pairs(Workspace.Items:GetChildren()) do
                if Obj:IsA("Model") and Obj.PrimaryPart then
                    table.insert(items, Obj)
                end
            end
            CollectItems(items, "–í—Å–µ –ø—Ä–µ–¥–º–µ—Ç—ã")
        end},
        {"ü™µ –í—Å–µ –±—Ä–µ–≤–Ω–∞", function() CollectByName("Log") end},
        {"‚ö´Ô∏è –í–µ—Å—å —É–≥–æ–ª—å", function() CollectByName("Coal") end},
        {"‚õΩÔ∏è –í—Å–µ –∫–∞–Ω–∏—Å—Ç—Ä—ã", function() CollectByName("Fuel Canister") end},
        {"ü©π –í—Å–µ –±–∏–Ω—Ç—ã", function() CollectByName("Bandage") end}
    }

    for _, itemData in ipairs(quickAccessItems) do
        BringItemTab:Button({
            Title = itemData[1],
            Desc = "–ë—ã—Å—Ç—Ä—ã–π —Å–±–æ—Ä",
            Callback = itemData[2]
        })
    end

    -- –°–µ–∫—Ü–∏—è –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
    BringItemTab:Section({Title = "üõ†Ô∏è –ú–∞—Ç–µ—Ä–∏–∞–ª—ã"})

    local materialItems = {
        {"üî© –ú–µ—Ç–∞–ª–ª–∏—á–µ—Å–∫–∏–µ –ª–∏—Å—Ç—ã", "Sheet Metal"},
        {"üß≤ –ë–æ–ª—Ç—ã", "Bolt"},
        {"üõû –®–∏–Ω—ã", "Tyre"},
        {"üåÄ –°–ª–æ–º–∞–Ω–Ω—ã–µ –≤–µ–Ω—Ç–∏–ª—è—Ç–æ—Ä—ã", "Broken Fan"},
        {"üìª –°—Ç–∞—Ä—ã–µ —Ä–∞–¥–∏–æ", "Old Radio"},
        {"üß∞ –ö–æ—Ä–æ–±–∫–∏ —Å–µ–º—è–Ω", "Seed Box"}
    }

    for _, itemData in ipairs(materialItems) do
        BringItemTab:Button({
            Title = itemData[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å " .. itemData[2],
            Callback = function() CollectByName(itemData[2]) end
        })
    end

    -- –°–µ–∫—Ü–∏—è —Ä–µ—Å—É—Ä—Å–æ–≤
    BringItemTab:Section({Title = "üìö –†–µ—Å—É—Ä—Å—ã"})

    local resourceGroups = {
        {"üõ¢Ô∏è –í—Å–µ —Ç–æ–ø–ª–∏–≤–æ", {"Log", "Fuel Canister", "Coal", "Oil Barrel"}},
        {"üî´ –í—Å–µ –ø–∞—Ç—Ä–æ–Ω—ã", {"Rifle Ammo", "Revolver Ammo"}},
        {"üçé –í—Å—è –µ–¥–∞", {"Cake", "Carrot", "Morsel", "Meat? Sandwich"}},
        {"üî© –í—Å–µ –∑–∞–ø—á–∞—Å—Ç–∏", {"Tyre", "Sheet Metal", "Broken Fan", "Bolt", "Old Radio", "UFO Junk", "UFO Scrap", "Broken Microwave"}}
    }

    for _, groupData in ipairs(resourceGroups) do
        BringItemTab:Button({
            Title = groupData[1],
            Desc = "–°–æ–±—Ä–∞—Ç—å –≥—Ä—É–ø–ø—É",
            Callback = function() CollectByType(groupData[2]) end
        })
    end

    -- –°–µ–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞
    BringItemTab:Section({Title = "üîç –ü–æ–∏—Å–∫ –ø—Ä–µ–¥–º–µ—Ç–æ–≤"})

    -- –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ–∏—Å–∫–∞
    local searchStats = BringItemTab:Label({
        Title = "–†–µ–∑—É–ª—å—Ç–∞—Ç—ã –ø–æ–∏—Å–∫–∞",
        Desc = "–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞ –¥–ª—è –ø–æ–∏—Å–∫–∞"
    })

    -- –ü–æ–ª–µ –ø–æ–∏—Å–∫–∞
    local searchBox = BringItemTab:Textbox({
        Title = "–ù–∞–∑–≤–∞–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–∞",
        Desc = "–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ ESP –¥–ª—è —Ç–æ—á–Ω–æ–≥–æ –Ω–∞–∑–≤–∞–Ω–∏—è",
        Placeholder = "–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ...",
        Value = "",
        ClearText = false,
        Callback = function(text)
            TextBoxText = text
            SearchResults = {}
            
            if text and text ~= "" then
                local count = 0
                local positions = {}
                
                for _, Obj in pairs(Workspace.Items:GetChildren()) do
                    if Obj.Name == text and Obj:IsA("Model") and Obj.PrimaryPart then 
                        count = count + 1
                        table.insert(SearchResults, Obj)
                        table.insert(positions, Obj.PrimaryPart.Position)
                    end 
                end
                
                searchStats:SetDesc(string.format("–ù–∞–π–¥–µ–Ω–æ: %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤\n–ù–∞–∑–≤–∞–Ω–∏–µ: %s", count, text))
                
                if count > 0 then
                    Window:Notify({
                        Title = "üîç –†–µ–∑—É–ª—å—Ç–∞—Ç—ã –ø–æ–∏—Å–∫–∞",
                        Desc = string.format("–ù–∞–π–¥–µ–Ω–æ %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤: %s", count, text),
                        Time = 3
                    })
                end
            end
        end
    })

    -- –ö–Ω–æ–ø–∫–∏ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–æ–∏—Å–∫–æ–º
    BringItemTab:Button({
        Title = "üì¶ –°–æ–±—Ä–∞—Ç—å –Ω–∞–π–¥–µ–Ω–Ω–æ–µ",
        Desc = "–°–æ–±—Ä–∞—Ç—å –≤—Å–µ –Ω–∞–π–¥–µ–Ω–Ω—ã–µ –ø—Ä–µ–¥–º–µ—Ç—ã",
        Callback = function()
            if #SearchResults > 0 then
                CollectItems(SearchResults, TextBoxText)
            else
                Window:Notify({
                    Title = "‚ö†Ô∏è –ù–µ—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤",
                    Desc = "–°–Ω–∞—á–∞–ª–∞ –≤—ã–ø–æ–ª–Ω–∏—Ç–µ –ø–æ–∏—Å–∫ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Time = 3
                })
            end
        end
    })

    BringItemTab:Button({
        Title = "üìç –ü–æ–∫–∞–∑–∞—Ç—å –Ω–∞ –∫–∞—Ä—Ç–µ",
        Desc = "–ü–æ–∫–∞–∑–∞—Ç—å —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
        Callback = function()
            if #SearchResults > 0 then
                Window:Notify({
                    Title = "üìç –†–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    Desc = string.format("–ù–∞–π–¥–µ–Ω–æ %d –ø—Ä–µ–¥–º–µ—Ç–æ–≤ —Ç–∏–ø–∞: %s", #SearchResults, TextBoxText),
                    Time = 4
                })
                
                -- –í–∫–ª—é—á–∞–µ–º ESP –¥–ª—è –Ω–∞–π–¥–µ–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
                for _, item in ipairs(SearchResults) do
                    if not item:FindFirstChildOfClass("Highlight") then
                        local highlight = Instance.new("Highlight")
                        highlight.Name = "Search_Highlight"
                        highlight.Adornee = item
                        highlight.FillColor = Color3.fromRGB(255, 0, 255)
                        highlight.FillTransparency = 0.5
                        highlight.OutlineColor = Color3.fromRGB(255, 0, 255)
                        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                        highlight.Parent = item
                        
                        -- –£–±–∏—Ä–∞–µ–º —á–µ—Ä–µ–∑ 10 —Å–µ–∫—É–Ω–¥
                        task.delay(10, function()
                            if highlight then
                                highlight:Destroy()
                            end
                        end)
                    end
                end
            end
        end
    })

    -- –°–µ–∫—Ü–∏—è —Å–ø–µ—Ü–∏–∞–ª—å–Ω–æ–≥–æ —Å–±–æ—Ä–∞
    BringItemTab:Section({Title = "üéØ –°–ø–µ—Ü–∏–∞–ª—å–Ω—ã–π —Å–±–æ—Ä"})

    BringItemTab:Button({
        Title = "üë∂ –°–æ–±—Ä–∞—Ç—å –¥–µ—Ç–µ–π",
        Desc = "–ü–µ—Ä–µ–Ω–µ—Å—Ç–∏ –≤—Å–µ—Ö –ø–æ—Ç–µ—Ä—è–Ω–Ω—ã—Ö –¥–µ—Ç–µ–π",
        Callback = function()
            local children = {}
            for _, Obj in pairs(Workspace.Characters:GetChildren()) do
                if Obj:IsA("Model") and Obj.PrimaryPart and 
                   (Obj.Name == "Lost Child" or Obj.Name == "Lost Child2" or 
                    Obj.Name == "Lost Child3" or Obj.Name == "Lost Child4") then 
                    table.insert(children, Obj)
                end  
            end
            
            if #children > 0 then
                CollectItems(children, "–î–µ—Ç–∏")
            else
                Window:Notify({
                    Title = "‚ö†Ô∏è –î–µ—Ç–∏ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã",
                    Desc = "–ù–∞ –∫–∞—Ä—Ç–µ –Ω–µ—Ç –ø–æ—Ç–µ—Ä—è–Ω–Ω—ã—Ö –¥–µ—Ç–µ–π",
                    Time = 3
                })
            end
        end
    })

    BringItemTab:Button({
        Title = "ü¶ä –°–æ–±—Ä–∞—Ç—å —Ç–æ—Ä–≥–æ–≤—Ü–∞",
        Desc = "–ù–∞–π—Ç–∏ –∏ —Å–æ–±—Ä–∞—Ç—å —Ç–æ—Ä–≥–æ–≤—Ü–∞",
        Callback = function()
            local traders = {}
            for _, Obj in pairs(Workspace.Characters:GetChildren()) do
                if Obj:IsA("Model") and Obj.PrimaryPart and Obj.Name == "Pelt Trader" then 
                    table.insert(traders, Obj)
                end  
            end
            
            if #traders > 0 then
                CollectItems(traders, "–¢–æ—Ä–≥–æ–≤–µ—Ü")
            else
                Window:Notify({
                    Title = "‚ö†Ô∏è –¢–æ—Ä–≥–æ–≤–µ—Ü –Ω–µ –Ω–∞–π–¥–µ–Ω",
                    Desc = "–¢–æ—Ä–≥–æ–≤–µ—Ü –º–µ—Ö–æ–º –Ω–µ –Ω–∞–π–¥–µ–Ω –Ω–∞ –∫–∞—Ä—Ç–µ",
                    Time = 3
                })
            end
        end
    })

    -- –°–µ–∫—Ü–∏—è –Ω–∞—Å—Ç—Ä–æ–µ–∫ —Å–±–æ—Ä–∞
    BringItemTab:Section({Title = "‚öôÔ∏è –ù–∞—Å—Ç—Ä–æ–π–∫–∏ —Å–±–æ—Ä–∞"})

    local autoReturnToggle = false
    BringItemTab:Toggle({
        Title = "üîÑ –ê–≤—Ç–æ–≤–æ–∑–≤—Ä–∞—Ç",
        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –≤–æ–∑–≤—Ä–∞—â–∞—Ç—å—Å—è –Ω–∞ –º–µ—Å—Ç–æ",
        Value = true,
        Callback = function(v)
            autoReturnToggle = v
        end
    })

    local collectDelay = 0.5
    BringItemTab:Slider({
        Title = "‚è±Ô∏è –ó–∞–¥–µ—Ä–∂–∫–∞ —Å–±–æ—Ä–∞",
        Desc = "–ü–∞—É–∑–∞ –º–µ–∂–¥—É —Å–±–æ—Ä–æ–º –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
        Min = 0.1,
        Max = 2,
        Rounding = 1,
        Value = 0.5,
        Callback = function(val)
            collectDelay = val
        end
    })

    -- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
    BringItemTab:Section({Title = "üìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞"})

    local infoPanel = BringItemTab:Label({
        Title = "–°—Ç–∞—Ç—É—Å —Å–±–æ—Ä–∞",
        Desc = "–ì–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ"
    })

    -- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
    local function UpdateStats()
        local itemCount = 0
        for _ in pairs(Workspace.Items:GetChildren()) do
            itemCount = itemCount + 1
        end
        
        local charCount = 0
        for _, char in pairs(Workspace.Characters:GetChildren()) do
            if char:IsA("Model") then
                charCount = charCount + 1
            end
        end
        
        infoPanel:SetDesc(string.format("–ü—Ä–µ–¥–º–µ—Ç–æ–≤ –Ω–∞ –∫–∞—Ä—Ç–µ: %d\n–ü–µ—Ä—Å–æ–Ω–∞–∂–µ–π: %d", itemCount, charCount))
    end

    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –∫–∞–∂–¥—ã–µ 10 —Å–µ–∫—É–Ω–¥
    task.spawn(function()
        while true do
            UpdateStats()
            wait(10)
        end
    end)

    -- –ö–Ω–æ–ø–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    BringItemTab:Button({
        Title = "üîÑ –û–±–Ω–æ–≤–∏—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É",
        Desc = "–û–±–Ω–æ–≤–∏—Ç—å –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –ø—Ä–µ–¥–º–µ—Ç–∞—Ö",
        Callback = UpdateStats
    })
end
