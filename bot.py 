import asyncio
import time
import re
from telethon import TelegramClient
from telegram import Update
from telegram.ext import Application, CommandHandler, MessageHandler, filters, ContextTypes

print("=" * 70)
print("ü§ñ TELEGRAM –ë–û–¢ –î–õ–Ø –ö–õ–Æ–ß–ï–ô")
print("=" * 70)

# ========== –ù–ê–°–¢–†–û–ô–ö–ò ==========
# –¢–í–û–ò USER API (–¥–ª—è @kuliscriptbot)
USER_API_ID = 32711756
USER_API_HASH = '788fb0beaa018613251d75575d769fed'
TARGET_BOT = '@kuliscriptbot'

# –¢–û–ö–ï–ù –¢–í–û–ï–ì–û –ë–û–¢–ê (–ø–æ–ª—É—á–∏ —É @BotFather)
BOT_TOKEN = "8467141821:AAHPwF9_-GTcojQqXfBpVR234j6vyzMMX68"

# ========== USER CLIENT (–¥–ª—è —Ä–∞–±–æ—Ç—ã —Å @kuliscriptbot) ==========
class KuliscriptProcessor:
    def __init__(self):
        self.client = None
        self.user_queues = {}  # –û—á–µ—Ä–µ–¥–∏ –¥–ª—è –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
        self.user_cooldowns = {}  # –í—Ä–µ–º—è –ø–æ—Å–ª–µ–¥–Ω–µ–≥–æ –∑–∞–ø—Ä–æ—Å–∞
        
    async def start(self):
        """–ó–∞–ø—É—Å–∫ –∫–ª–∏–µ–Ω—Ç–∞"""
        try:
            self.client = TelegramClient('kuli_processor', USER_API_ID, USER_API_HASH)
            await self.client.start()
            print("‚úÖ User client started")
            return True
        except Exception as e:
            print(f"‚ùå –û—à–∏–±–∫–∞: {e}")
            return False
    
    async def find_button(self):
        """–ù–∞–π—Ç–∏ –∫–Ω–æ–ø–∫—É '–û–±–æ–π—Ç–∏ –∫–ª—é—á'"""
        try:
            bot = await self.client.get_entity(TARGET_BOT)
            await self.client.send_message(bot, '/start')
            await asyncio.sleep(2)
            
            messages = await self.client.get_messages(bot, limit=5)
            
            for msg in messages:
                if not msg.out and msg.buttons:
                    for row in msg.buttons:
                        for button in row:
                            if any(x in button.text.lower() for x in ['–æ–±–æ–π—Ç–∏', 'bypass', '–∫–ª—é—á']):
                                return button.text
            return None
        except:
            return None
    
    def can_send(self, user_id):
        """–ú–æ–∂–Ω–æ –ª–∏ –æ—Ç–ø—Ä–∞–≤–ª—è—Ç—å –∑–∞–ø—Ä–æ—Å"""
        if user_id not in self.user_cooldowns:
            return True, 0
        
        current_time = time.time()
        time_since_last = current_time - self.user_cooldowns[user_id]
        
        if time_since_last < 60:  # 60 —Å–µ–∫—É–Ω–¥ –ø–∞—É–∑–∞
            wait_time = 60 - time_since_last
            return False, wait_time
        
        return True, 0
    
    async def process_link_for_user(self, user_id, link):
        """–û–±—Ä–∞–±–æ—Ç–∞—Ç—å —Å—Å—ã–ª–∫—É –∏ –ø–æ–ª—É—á–∏—Ç—å –∫–ª—é—á"""
        try:
            # –ü—Ä–æ–≤–µ—Ä—è–µ–º –ø–∞—É–∑—É
            can_send, wait_time = self.can_send(user_id)
            if not can_send:
                return f"wait:{int(wait_time)}"
            
            bot = await self.client.get_entity(TARGET_BOT)
            
            # 1. –ù–∞–∂–∏–º–∞–µ–º –∫–Ω–æ–ø–∫—É
            button_text = await self.find_button()
            if button_text:
                messages = await self.client.get_messages(bot, limit=5)
                for msg in messages:
                    if not msg.out and msg.buttons:
                        for row in msg.buttons:
                            for button in row:
                                if button.text == button_text:
                                    await msg.click(text=button_text)
                                    await asyncio.sleep(2)
                                    break
            
            # 2. –û—Ç–ø—Ä–∞–≤–ª—è–µ–º —Å—Å—ã–ª–∫—É
            await self.client.send_message(bot, link)
            self.user_cooldowns[user_id] = time.time()
            
            # 3. –ñ–¥–µ–º 45 —Å–µ–∫—É–Ω–¥
            await asyncio.sleep(45)
            
            # 4. –ü–æ–ª—É—á–∞–µ–º –ø–æ—Å–ª–µ–¥–Ω–∏–µ —Å–æ–æ–±—â–µ–Ω–∏—è
            messages = await self.client.get_messages(bot, limit=5)
            
            # 5. –ò—â–µ–º –ö–õ–Æ–ß (—Ç–æ–ª—å–∫–æ –∫–ª—é—á!)
            for msg in messages:
                if not msg.out:
                    text = msg.text
                    
                    # –ò—â–µ–º –∫–ª—é—á FREE_xxxxxxxx
                    key_match = re.search(r'(FREE|KEY_)[a-fA-F0-9]{20,}', text)
                    if key_match:
                        return key_match.group(0)
                    
                    # –ò—â–µ–º —Å—Å—ã–ª–∫—É
                    url_match = re.search(r'https?://[^\s]+', text)
                    if url_match:
                        return url_match.group(0)
            
            return None
            
        except Exception as e:
            print(f"‚ùå –û—à–∏–±–∫–∞ –æ–±—Ä–∞–±–æ—Ç–∫–∏: {e}")
            return None

# ========== TELEGRAM –ë–û–¢ ==========
class CleanKeyBot:
    def __init__(self):
        self.processor = KuliscriptProcessor()
        self.bot_app = None
        
    async def start(self):
        """–ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º—ã"""
        if not await self.processor.start():
            return False
        
        self.bot_app = Application.builder().token(BOT_TOKEN).build()
        
        # –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏
        self.bot_app.add_handler(CommandHandler("start", self.start_cmd))
        self.bot_app.add_handler(CommandHandler("help", self.help_cmd))
        self.bot_app.add_handler(MessageHandler(filters.TEXT & ~filters.COMMAND, self.handle_message))
        
        print("‚úÖ –ë–æ—Ç –∑–∞–ø—É—â–µ–Ω!")
        return True
    
    async def start_cmd(self, update: Update, context: ContextTypes.DEFAULT_TYPE):
        """–ö–æ–º–∞–Ω–¥–∞ /start"""
        await update.message.reply_text(
            "üîë *–ë–æ—Ç –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∫–ª—é—á–µ–π*\n\n"
            "–ü—Ä–æ—Å—Ç–æ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –º–Ω–µ Delta —Å—Å—ã–ª–∫—É.\n"
            "–Ø –æ—Ç–ø—Ä–∞–≤–ª—é –µ—ë –≤ @kuliscriptbot –∏ –ø—Ä–∏—à–ª—é –≤–∞–º –∫–ª—é—á.\n\n"
            "‚è∞ *–ü–∞—É–∑–∞ –º–µ–∂–¥—É –∑–∞–ø—Ä–æ—Å–∞–º–∏:* 60 —Å–µ–∫—É–Ω–¥\n"
            "üîÑ *–†–∞–±–æ—Ç–∞–µ—Ç —Å:* loot-link.com, delta-app.com\n\n"
            "üìé *–ü—Ä–∏–º–µ—Ä —Å—Å—ã–ª–∫–∏:*\n"
            "`https://loot-link.com/?r=Base64Code`",
            parse_mode='Markdown'
        )
    
    async def help_cmd(self, update: Update, context: ContextTypes.DEFAULT_TYPE):
        """–ö–æ–º–∞–Ω–¥–∞ /help"""
        await update.message.reply_text(
            "üìã *–ö–∞–∫ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å:*\n"
            "1. –û—Ç–ø—Ä–∞–≤—å—Ç–µ Delta —Å—Å—ã–ª–∫—É\n"
            "2. –ë–æ—Ç –æ–±—Ä–∞–±–æ—Ç–∞–µ—Ç –µ—ë\n"
            "3. –í—ã –ø–æ–ª—É—á–∏—Ç–µ –∫–ª—é—á\n\n"
            "‚è∞ *–ü–∞—É–∑–∞:* 60 —Å–µ–∫—É–Ω–¥ –º–µ–∂–¥—É –∑–∞–ø—Ä–æ—Å–∞–º–∏\n"
            "üîë *–§–æ—Ä–º–∞—Ç –∫–ª—é—á–∞:* FREE_xxxxxxxx\n\n"
            "‚ùì *–ü—Ä–æ–±–ª–µ–º—ã?* –ü–µ—Ä–µ–∑–∞–ø—É—Å—Ç–∏—Ç–µ –±–æ—Ç–∞ –∫–æ–º–∞–Ω–¥–æ–π /start"
        )
    
    async def handle_message(self, update: Update, context: ContextTypes.DEFAULT_TYPE):
        """–û–±—Ä–∞–±–æ—Ç–∫–∞ —Å–æ–æ–±—â–µ–Ω–∏–π"""
        user_id = update.effective_user.id
        text = update.message.text
        
        # –ò–≥–Ω–æ—Ä–∏—Ä—É–µ–º –∫–æ–º–∞–Ω–¥—ã
        if text.startswith('/'):
            return
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—Å—ã–ª–∫—É
        if not ('http://' in text or 'https://' in text):
            await update.message.reply_text("‚ùå –û—Ç–ø—Ä–∞–≤—å—Ç–µ —Å—Å—ã–ª–∫—É (–Ω–∞—á–∏–Ω–∞–µ—Ç—Å—è —Å http)")
            return
        
        # –°—Ç–∞—Ç—É—Å
        status_msg = await update.message.reply_text("üîÑ *–û–±—Ä–∞–±–∞—Ç—ã–≤–∞—é...*")
        
        try:
            # –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º
            result = await self.processor.process_link_for_user(user_id, text)
            
            if result and result.startswith("wait:"):
                wait_seconds = int(result.split(":")[1])
                await status_msg.edit_text(f"‚è≥ *–ü–æ–¥–æ–∂–¥–∏—Ç–µ {wait_seconds} —Å–µ–∫—É–Ω–¥*")
            
            elif result:
                # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –¢–û–õ–¨–ö–û –∫–ª—é—á/—Å—Å—ã–ª–∫—É
                await status_msg.edit_text(f"‚úÖ *–ö–ª—é—á –ø–æ–ª—É—á–µ–Ω:*\n`{result}`")
                
                # –î–æ–ø. –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –µ—Å–ª–∏ —ç—Ç–æ —Å—Å—ã–ª–∫–∞
                if 'http' in result and 'pastebin.com' in result:
                    raw_link = result.replace('pastebin.com/', 'pastebin.com/raw/')
                    await update.message.reply_text(f"‚ö° *–ü—Ä—è–º–∞—è —Å—Å—ã–ª–∫–∞:*\n`{raw_link}`")
            
            else:
                await status_msg.edit_text("‚ùå *–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å –∫–ª—é—á*")
                
        except Exception as e:
            await status_msg.edit_text(f"‚ùå *–û—à–∏–±–∫–∞:* `{str(e)[:50]}`")
    
    async def run(self):
        """–ó–∞–ø—É—Å–∫ –±–æ—Ç–∞"""
        await self.bot_app.initialize()
        await self.bot_app.start()
        await self.bot_app.updater.start_polling()
        
        print("üì± –ë–æ—Ç –∑–∞–ø—É—â–µ–Ω –≤ Telegram!")
        print("‚è≥ –û–∂–∏–¥–∞—é —Å–æ–æ–±—â–µ–Ω–∏—è...")
        
        while True:
            await asyncio.sleep(1)

# ========== –ó–ê–ü–£–°–ö ==========
async def main():
    if BOT_TOKEN == "–í–ê–®_–¢–û–ö–ï–ù_–ë–û–¢–ê":
        print("‚ùå –ó–ê–ú–ï–ù–ò–¢–ï BOT_TOKEN!")
        print("\nüìù –ü–æ–ª—É—á–∏—Ç–µ —É @BotFather")
        return
    
    bot = CleanKeyBot()
    
    if await bot.start():
        await bot.run()

if __name__ == "__main__":
    try:
        from telethon import TelegramClient
        from telegram import Update
        
        print("üöÄ –ó–∞–ø—É—Å–∫–∞—é –±–æ—Ç–∞...")
        asyncio.run(main())
        
    except ImportError:
        print("‚ùå –£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ –±–∏–±–ª–∏–æ—Ç–µ–∫–∏:")
        print("1. telethon")
        print("2. python-telegram-bot")
        print("\nüì¶ Pydroid ‚Üí –ú–µ–Ω—é ‚Üí Pip ‚Üí —É—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ –æ–±–µ")