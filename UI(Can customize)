local Tab = Window:Tab({Title = "</> Executor", Icon = "code"}) do
    
    local currentScript = "-- –í–≤–µ–¥–∏—Ç–µ –≤–∞—à Lua —Å–∫—Ä–∏–ø—Ç –∑–¥–µ—Å—å\nprint('Hello Saul Hub!')"
    local scriptHistory = {}
    
    Tab:Section({Title = "‚ö° Script Executor"})

    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º Textbox –¥–ª—è –≤–≤–æ–¥–∞ —Å–∫—Ä–∏–ø—Ç–∞
    local scriptInput = Tab:Textbox({
        Title = "üìù –í–≤–µ–¥–∏—Ç–µ —Å–∫—Ä–∏–ø—Ç",
        Desc = "–ù–∞–ø–∏—à–∏—Ç–µ –∏–ª–∏ –≤—Å—Ç–∞–≤—å—Ç–µ Lua –∫–æ–¥ –¥–ª—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è",
        Value = currentScript,
        Placeholder = "print('Hello World!')",
        ClearText = false,
        Callback = function(text)
            currentScript = text
        end
    })

    -- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ —Å–∫—Ä–∏–ø—Ç–µ
    local scriptInfo = Tab:Label({
        Title = "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ —Å–∫—Ä–∏–ø—Ç–µ",
        Desc = "–°–∏–º–≤–æ–ª–æ–≤: " .. #currentScript
    })

    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ —Å–∫—Ä–∏–ø—Ç–∞
    local function updateScriptInfo()
        local charCount = #currentScript
        local lineCount = #string.split(currentScript, "\n")
        scriptInfo:SetDesc(string.format("–°–∏–º–≤–æ–ª–æ–≤: %d | –°—Ç—Ä–æ–∫: %d", charCount, lineCount))
    end

    -- –£–ü–†–ê–í–õ–ï–ù–ò–ï –í–´–ü–û–õ–ù–ï–ù–ò–ï–ú
    Tab:Section({Title = "‚öôÔ∏è –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ–º"})

    local executionHistory = {}

    -- –ö–Ω–æ–ø–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è
    Tab:Button({
        Title = "üöÄ –í—ã–ø–æ–ª–Ω–∏—Ç—å —Å–∫—Ä–∏–ø—Ç",
        Desc = "–ó–∞–ø—É—Å—Ç–∏—Ç—å —Ç–µ–∫—É—â–∏–π –∫–æ–¥",
        Callback = function()
            btnclick()
            
            if not currentScript or currentScript:gsub("%s", "") == "" then
                Window:Notify({
                    Title = "‚ö†Ô∏è –í–Ω–∏–º–∞–Ω–∏–µ",
                    Desc = "–°–∫—Ä–∏–ø—Ç –ø—É—Å—Ç–æ–π!",
                    Time = 3
                })
                return
            end

            -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –≤ –∏—Å—Ç–æ—Ä–∏—é
            table.insert(scriptHistory, currentScript)
            if #scriptHistory > 10 then
                table.remove(scriptHistory, 1)
            end

            -- –õ–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ
            print("üîÑ –í—ã–ø–æ–ª–Ω–µ–Ω–∏–µ —Å–∫—Ä–∏–ø—Ç–∞ Saul Hub...")
            print("üìè –î–ª–∏–Ω–∞ –∫–æ–¥–∞: " .. #currentScript .. " —Å–∏–º–≤–æ–ª–æ–≤")

            -- –ë–µ–∑–æ–ø–∞—Å–Ω–æ–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ
            local success, result = pcall(function()
                local fn, err = loadstring(currentScript)
                if fn then
                    return fn()
                else
                    error(err)
                end
            end)

            -- –ó–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–π
            local executionRecord = {
                script = currentScript,
                success = success,
                result = result,
                time = os.time()
            }
            table.insert(executionHistory, executionRecord)

            if success then
                Window:Notify({
                    Title = "‚úÖ –£—Å–ø–µ—Ö!",
                    Desc = "–°–∫—Ä–∏–ø—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω —É—Å–ø–µ—à–Ω–æ",
                    Time = 3
                })
                print("‚úÖ –°–∫—Ä–∏–ø—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω —É—Å–ø–µ—à–Ω–æ!")
            else
                local errorMsg = tostring(result):sub(1, 100)
                Window:Notify({
                    Title = "‚ùå –û—à–∏–±–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è",
                    Desc = errorMsg .. "...",
                    Time = 5
                })
                warn("‚ùå –û—à–∏–±–∫–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è:", result)
            end
        end
    })

    -- –ö–Ω–æ–ø–∫–∞ –æ—á–∏—Å—Ç–∫–∏
    Tab:Button({
        Title = "üóëÔ∏è –û—á–∏—Å—Ç–∏—Ç—å –ø–æ–ª–µ",
        Desc = "–û—á–∏—Å—Ç–∏—Ç—å –ø–æ–ª–µ –≤–≤–æ–¥–∞ —Å–∫—Ä–∏–ø—Ç–∞",
        Callback = function()
            btnclick()
            currentScript = ""
            scriptInput:SetValue("")
            updateScriptInfo()
            Window:Notify({
                Title = "üßπ –û—á–∏—â–µ–Ω–æ",
                Desc = "–ü–æ–ª–µ –≤–≤–æ–¥–∞ –æ—á–∏—â–µ–Ω–æ",
                Time = 2
            })
        end
    })

    -- –ë–´–°–¢–†–´–ï –°–ö–†–ò–ü–¢–´
    Tab:Section({Title = "üéØ –ë—ã—Å—Ç—Ä—ã–µ —Å–∫—Ä–∏–ø—Ç—ã"})

    local quickScripts = {
        {"üîÑ –ü–µ—Ä–µ–∑–∞–ø—É—Å–∫ –∏–≥—Ä—ã", 'game:GetService("TeleportService"):Teleport(game.PlaceId)'},
        {"üëÅÔ∏è ESP –ò–≥—Ä–æ–∫–æ–≤", [[
-- ESP Players
for _, player in pairs(game.Players:GetPlayers()) do
    if player ~= game.Players.LocalPlayer and player.Character then
        local highlight = Instance.new("Highlight")
        highlight.Parent = player.Character
        highlight.FillColor = Color3.new(1, 0, 0)
        highlight.Name = "SaulESP"
    end
end
]]},
        {"üöÄ –°—É–ø–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç—å", [[
-- Super Speed
local player = game.Players.LocalPlayer
if player.Character then
    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        humanoid.WalkSpeed = 100
    end
end
]]},
        {"üõ°Ô∏è Anti-AFK", [[
-- Anti-AFK
local VirtualUser = game:GetService('VirtualUser')
game:GetService('Players').LocalPlayer.Idled:connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)
]]},
        {"üåÄ Noclip", [[
-- Noclip
local noclip = false
local player = game.Players.LocalPlayer

game:GetService('RunService').Stepped:Connect(function()
    if noclip and player.Character then
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)
]]}
    }

    for i, scriptData in ipairs(quickScripts) do
        Tab:Button({
            Title = scriptData[1],
            Desc = "–í—Å—Ç–∞–≤–∏—Ç—å —à–∞–±–ª–æ–Ω —Å–∫—Ä–∏–ø—Ç–∞",
            Callback = function()
                btnclick()
                currentScript = scriptData[2]
                scriptInput:SetValue(currentScript)
                updateScriptInfo()
                Window:Notify({
                    Title = "üéØ –®–∞–±–ª–æ–Ω –∑–∞–≥—Ä—É–∂–µ–Ω",
                    Desc = scriptData[1] .. " –≤—Å—Ç–∞–≤–ª–µ–Ω",
                    Time = 3
                })
            end
        })
    end

    -- –ê–õ–¨–¢–ï–†–ù–ê–¢–ò–í–ù–´–ï –ú–ï–¢–û–î–´
    Tab:Section({Title = "üì• –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–µ –º–µ—Ç–æ–¥—ã"})

    -- –ó–∞–≥—Ä—É–∑–∫–∞ –∏–∑ —Ñ–∞–π–ª–∞ (–µ—Å–ª–∏ –¥–æ—Å—Ç—É–ø–Ω–æ)
    if readfile and writefile then
        Tab:Button({
            Title = "üìÅ –ó–∞–≥—Ä—É–∑–∏—Ç—å –∏–∑ —Ñ–∞–π–ª–∞",
            Desc = "–ó–∞–≥—Ä—É–∑–∏—Ç—å —Å–∫—Ä–∏–ø—Ç –∏–∑ saul_script.lua",
            Callback = function()
                btnclick()
                local success, content = pcall(readfile, "saul_script.lua")
                if success then
                    currentScript = content
                    scriptInput:SetValue(currentScript)
                    updateScriptInfo()
                    Window:Notify({
                        Title = "‚úÖ –£—Å–ø–µ—Ö",
                        Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω –∏–∑ —Ñ–∞–π–ª–∞",
                        Time = 3
                    })
                else
                    -- –°–æ–∑–¥–∞–µ–º –ø—Ä–∏–º–µ—Ä —Ñ–∞–π–ª–∞
                    writefile("saul_script.lua", "-- –í–≤–µ–¥–∏—Ç–µ –≤–∞—à —Å–∫—Ä–∏–ø—Ç —Å—é–¥–∞\nprint('Hello Saul Hub!')")
                    Window:Notify({
                        Title = "üìÅ –§–∞–π–ª —Å–æ–∑–¥–∞–Ω",
                        Desc = "–§–∞–π–ª 'saul_script.lua' —Å–æ–∑–¥–∞–Ω\n–û—Ç—Ä–µ–¥–∞–∫—Ç–∏—Ä—É–π—Ç–µ –µ–≥–æ –∏ –Ω–∞–∂–º–∏—Ç–µ —Å–Ω–æ–≤–∞",
                        Time = 5
                    })
                end
            end
        })
    end

    -- –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –≤ —Ñ–∞–π–ª
    if writefile then
        Tab:Button({
            Title = "üíæ –°–æ—Ö—Ä–∞–Ω–∏—Ç—å –≤ —Ñ–∞–π–ª",
            Desc = "–°–æ—Ö—Ä–∞–Ω–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–∫—Ä–∏–ø—Ç –≤ —Ñ–∞–π–ª",
            Callback = function()
                btnclick()
                local success = pcall(writefile, "saul_script.lua", currentScript)
                if success then
                    Window:Notify({
                        Title = "üíæ –£—Å–ø–µ—Ö",
                        Desc = "–°–∫—Ä–∏–ø—Ç —Å–æ—Ö—Ä–∞–Ω–µ–Ω –≤ —Ñ–∞–π–ª",
                        Time = 3
                    })
                else
                    Window:Notify({
                        Title = "‚ùå –û—à–∏–±–∫–∞",
                        Desc = "–ù–µ —É–¥–∞–ª–æ—Å—å —Å–æ—Ö—Ä–∞–Ω–∏—Ç—å —Ñ–∞–π–ª",
                        Time = 3
                    })
                end
            end
        })
    end

    -- –ò–ù–§–û–†–ú–ê–¶–ò–Ø –ò –°–û–í–ï–¢–´
    Tab:Section({Title = "üìö –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è"})

    Tab:Label({
        Title = "üí° –ö–∞–∫ –ø–æ–ª—å–∑–æ–≤–∞—Ç—å—Å—è:",
        Desc = "1. –í–≤–µ–¥–∏—Ç–µ Lua –∫–æ–¥ –≤ –ø–æ–ª–µ –≤—ã—à–µ\n2. –ù–∞–∂–º–∏—Ç–µ '–í—ã–ø–æ–ª–Ω–∏—Ç—å —Å–∫—Ä–∏–ø—Ç'\n3. –°–ª–µ–¥–∏—Ç–µ –∑–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è–º–∏ –∏ –∫–æ–Ω—Å–æ–ª—å—é"
    })

    Tab:Label({
        Title = "üîß –î–æ—Å—Ç—É–ø–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏:",
        Desc = "‚Ä¢ loadstring() - –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –∫–æ–¥–∞\n‚Ä¢ game:GetService() - –¥–æ—Å—Ç—É–ø –∫ —Å–µ—Ä–≤–∏—Å–∞–º\n‚Ä¢ print() - –≤—ã–≤–æ–¥ –≤ –∫–æ–Ω—Å–æ–ª—å\n‚Ä¢ –ü–æ–ª–Ω—ã–π –¥–æ—Å—Ç—É–ø –∫ Roblox API"
    })

    Tab:Label({
        Title = "‚ö†Ô∏è –ë–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å:",
        Desc = "‚Ä¢ –í—Å–µ —Å–∫—Ä–∏–ø—Ç—ã –≤—ã–ø–æ–ª–Ω—è—é—Ç—Å—è –≤ –∑–∞—â–∏—â–µ–Ω–Ω–æ–º —Ä–µ–∂–∏–º–µ\n‚Ä¢ –û—à–∏–±–∫–∏ –æ—Ç–æ–±—Ä–∞–∂–∞—é—Ç—Å—è –≤ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è—Ö\n‚Ä¢ –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Ç–æ–ª—å–∫–æ –ø—Ä–æ–≤–µ—Ä–µ–Ω–Ω—ã–µ —Å–∫—Ä–∏–ø—Ç—ã"
    })

    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
    local function monitorScriptChanges()
        while task.wait(1) do
            updateScriptInfo()
        end
    end

    task.spawn(monitorScriptChanges)
end
--===========================================
-- –í–ö–õ–ê–î–ö–ê –ê–í–¢–û–ú–ê–¢–ò–ß–ï–°–ö–ò–• –°–ö–†–ò–ü–¢–û–í –ü–û –ò–ì–†–ï
--===========================================
local Tab = Window:Tab({Title = "üéØ Auto Scripts", Icon = "settings"}) do
    local currentGameId = game.PlaceId
    local currentGameData = GameDatabase[currentGameId]
    
    if currentGameData then
        Tab:Section({Title = "üéÆ –°–∫—Ä–∏–ø—Ç—ã –¥–ª—è: " .. currentGameData.name})
        
        for i, scriptData in ipairs(currentGameData.scripts) do
            Tab:Button({
                Title = scriptData[1],
                Desc = "–ó–∞–≥—Ä—É–∑–∏—Ç—å —Å–∫—Ä–∏–ø—Ç –¥–ª—è " .. currentGameData.name,
                Callback = function()
                    btnclick()
                    loadScriptSafe(scriptData[2])
                end
            })
        end
        
        Tab:Label({
            Title = "‚úÖ –û–±–Ω–∞—Ä—É–∂–µ–Ω–∞ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ–º–∞—è –∏–≥—Ä–∞",
            Desc = "ID –º–µ—Å—Ç–∞: " .. currentGameId
        })
    else
        Tab:Section({Title = "‚ùå –ò–≥—Ä–∞ –Ω–µ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è"})
        
        Tab:Label({
            Title = "–î–ª—è —ç—Ç–æ–π –∏–≥—Ä—ã –Ω–µ—Ç –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏—Ö —Å–∫—Ä–∏–ø—Ç–æ–≤",
            Desc = "ID –º–µ—Å—Ç–∞: " .. currentGameId .. "\n–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —É–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω—ã–µ —Å–∫—Ä–∏–ø—Ç—ã –∏–∑ –≥–ª–∞–≤–Ω–æ–π –≤–∫–ª–∞–¥–∫–∏"
        })
    end
    
    -- –°–ø–∏—Å–æ–∫ –≤—Å–µ—Ö –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ–º—ã—Ö –∏–≥—Ä
    Tab:Section({Title = "üìö –ü–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ–º—ã–µ –∏–≥—Ä—ã"})
    
    for gameId, gameData in pairs(GameDatabase) do
        Tab:Label({
            Title = "üéØ " .. gameData.name,
            Desc = "ID: " .. gameId
        })
    end
end

