local function loadScriptSafe(url)
    local success, result = pcall(function()
        return loadstring(game:HttpGet(url, true))()
    end)
    
    if success then
        Window:Notify({
            Title = "‚úÖ –£—Å–ø–µ—Ö",
            Desc = "–°–∫—Ä–∏–ø—Ç –∑–∞–≥—Ä—É–∂–µ–Ω!",
            Time = 3
        })
    else
        Window:Notify({
            Title = "‚ùå –û—à–∏–±–∫–∞",
            Desc = "–ù–µ —É–¥–∞–ª–æ—Å—å –∑–∞–≥—Ä—É–∑–∏—Ç—å —Å–∫—Ä–∏–ø—Ç",
            Time = 5
        })
        warn("–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏ —Å–∫—Ä–∏–ø—Ç–∞:", result)
    end
end

