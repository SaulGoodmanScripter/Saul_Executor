        Desc = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ —Å–∫—Ä–∏–ø—Ç—ã –¥–æ—Å—Ç—É–ø–Ω—ã –≤–æ –≤–∫–ª–∞–¥–∫–µ 'Auto Scripts'",
        Time = 7
    })
else
    Window:Notify({
        Title = "script made by Saul_Hub", 
        Desc = "ID –º–µ—Å—Ç–∞: " .. game.PlaceId .. "\n–ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —É–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω—ã–µ —Å–∫—Ä–∏–ø—Ç—ã",
        Time = 5
    })
end

